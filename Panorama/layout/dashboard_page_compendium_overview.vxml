<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_shared.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_compendium_pages.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_compendium_overview.vcss_c" />
	</styles>

	<script>
	</script>

	<DOTACompendiumOverviewPage class="DashboardPage" hittest="false">
		<Panel id="CompendiumBody" class="CompendiumBody">
			<Panel id="OverviewBackground"/>
			<Panel id="Spot1" class="SpotlightContainer">
				<DOTAScenePanel id="Beam" map="scenes/bluespotlight" camera="camera_1" hittest="false"/>
			</Panel>			
			<Panel id="Spot2" class="SpotlightContainer">
				<DOTAScenePanel id="Beam" map="scenes/bluespotlight" camera="camera_1" hittest="false"/>
			</Panel>
			<Panel id="Spot3" class="SpotlightContainer">
				<DOTAScenePanel id="Beam" map="scenes/bluespotlight" camera="camera_1" hittest="false"/>
			</Panel>			
			<Panel id="OverviewBackgroundArena"/>
			<Panel class="CompendiumTopSection">
				<Panel id="OverviewContent" class="LeftRightFlow">
					<Panel id="LeftBlock" class="TopBottomFlow">
						<Panel id="MajorLogo" />
						<Button id="ScheduleButton" class="BlueButton" onactivate="DOTAShowTournamentDetailsPage( 4266, 1, 4 )">
							<Panel id="ScheduleButtonContents" class="LeftRightFlow">
								<Label text="#DOTA_Compendium_View_Brackets"/>
								<Panel id="PopOut"/>
							</Panel>
						</Button>
					</Panel>

					<Panel class="OverviewColumn TopBottomFlow">
						<Panel class="OverviewBlock TopBottomFlow">
							<Label class="OverviewHeading" text="#DOTA_CompendiumRecap_OverviewDates"/>
							<Label class="OverviewDetails" text="#DOTA_CompendiumRecap_OverviewDetails"/>
              <Label class="OverviewDetails" text=""/>
						</Panel>
						<Panel class="OverviewBlock TopBottomFlow">
							<Label class="OverviewHeading" text="#DOTA_CompendiumRecap_OverviewVenue"/>
							<Label class="OverviewDetails" text="#DOTA_CompendiumRecap_OverviewVenueDetails"/>
						</Panel>
					</Panel>
					<Panel class="OverviewColumn TopBottomFlow">
						<Panel class="OverviewBlock TopBottomFlow">
							<Label class="OverviewHeading" text="#DOTA_CompendiumRecap_OverviewFormat"/>
							<Label class="OverviewDetails" text="#DOTA_CompendiumRecap_OverviewFormatDetails"/>
              <Label class="OverviewDetails" text="#DOTA_CompendiumRecap_OverviewFormatDetailsCont"/>
						</Panel>
						<Panel class="OverviewBlock TopBottomFlow">
							<Label class="OverviewHeading" text="#DOTA_CompendiumRecap_OverviewTeams"/>
							<Label class="OverviewDetails" text="#DOTA_CompendiumRecap_OverviewTeamDetails"/>
						</Panel>
					</Panel>					
				</Panel>
			</Panel>


			<Panel id="Spot4" class="SpotlightContainer">
				<DOTAScenePanel id="Beam" map="scenes/bluespotlight" camera="camera_1" hittest="false"/>
			</Panel>									

		</Panel>		
	</DOTACompendiumOverviewPage>
</root>
