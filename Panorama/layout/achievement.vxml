<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/winter2016/bp_winter2016.vcss_c" />
		<include src="s2r://panorama/styles/achievement.vcss_c" />

	</styles>



	<Achievement class="Achievement">
		<Panel id="FrontRowAchievement" class="AchievementCell Hidden">
			
			<Panel class="AchievementImageContainer">
				<Image id="AchievementImage"/>
				<Label text="#DOTA_SeasonPassAchievement_Description" class="AchievementDescription"/>
				<Panel class="AchievementProgressBarContainer">
					<ProgressBar id="AchievementProgressBar" value="0.5" />
					<Label id="AchievementStatus" text="#DOTA_SeasonPassAchievement_Status" class="AchievementStatus"/>
				</Panel>
			</Panel>

			<Panel class="AchievementFooter">
				<Panel class="AchievementFirstLine">


				</Panel>
			</Panel>
			<Panel class="CheckAndName">
				<Label text="#DOTA_SeasonPassAchievement_Name" class="AchievementName"/>
			</Panel>
			<Panel class="XPCircle">
				<Label text="#DOTA_SeasonPassAchievement_Reward" class="AchievementReward"/>
				<Panel class="PTS"/>
			</Panel>
			<Panel class="CompletedPanel">
				
				
			</Panel>
		</Panel>

    <Panel class="BackRowHittest">
      <Panel id="AchievementButton1" class="AchievementCircle" onmouseover="ToggleStyle( FrontRowAchievement, Hovered);" onmouseout="ToggleStyle( FrontRowAchievement, Hovered);"/>
      <Panel id="AchievementButton2" class="AchievementCircle" onmouseover="ToggleStyle( MidRowAchievement, Hovered);" onmouseout="ToggleStyle( MidRowAchievement, Hovered);"/>
      <Panel id="AchievementButton3" class="AchievementCircle" onmouseover="ToggleStyle( BackRowAchievement, Hovered);" onmouseout="ToggleStyle( BackRowAchievement, Hovered);"/>
    </Panel>

		<Panel id="MidRowAchievement" class="AchievementCell Hidden">
			<Panel class="CheckMark"/>
			<Panel class="AchievementImageContainer">
				<Image id="AchievementImage"/>
				<Label text="#DOTA_SeasonPassAchievement_Description" class="AchievementDescription"/>
				<Panel class="AchievementProgressBarContainer">
					<ProgressBar id="AchievementProgressBar" value="0.5" />
					<Label id="AchievementStatus" text="#DOTA_SeasonPassAchievement_Status" class="AchievementStatus"/>
				</Panel>
			</Panel>

			<Panel class="AchievementFooter">
				<Panel class="AchievementFirstLine">

				</Panel>
			</Panel>
			<Panel class="CheckAndName">
				<Label text="#DOTA_SeasonPassAchievement_Name" class="AchievementName"/>
			</Panel>
			<Panel class="XPCircle">
				<Label text="#DOTA_SeasonPassAchievement_Reward" class="AchievementReward"/>
				<Panel class="PTS"/>
			</Panel>
			<Panel class="CompletedPanel">
				
				
			</Panel>
		</Panel>


		<Panel id="BackRowAchievement" class="AchievementCell Hidden">
			<Panel class="AchievementImageContainer">
				<Image id="AchievementImage"/>
				<Label text="#DOTA_SeasonPassAchievement_Description" class="AchievementDescription"/>
				<Panel class="AchievementProgressBarContainer">
					<ProgressBar id="AchievementProgressBar" value="0.5" />
					<Label id="AchievementStatus" text="#DOTA_SeasonPassAchievement_Status" class="AchievementStatus"/>
				</Panel>
			</Panel>

			<Panel class="AchievementFooter">
				<Panel class="AchievementFirstLine">

				</Panel>
			</Panel>
			<Panel class="CheckAndName">
				<Label text="#DOTA_SeasonPassAchievement_Name" class="AchievementName"/>
			</Panel>
			<Panel class="XPCircle">
				<Label text="#DOTA_SeasonPassAchievement_Reward" class="AchievementReward"/>
				<Panel class="PTS"/>
			</Panel>
			<Panel class="CompletedPanel">
				
			</Panel>
		</Panel>

	</Achievement>



</root>
