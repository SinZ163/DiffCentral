<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/loading_screen.vcss_c" />
		<include src="s2r://panorama/styles/loading_screen_weekend_tourney.vcss_c" />
	</styles>

	<DOTALoadingScreenWeekendTourney onload="AsyncEvent(2.0,AddStyle(ShowBracket))">
		<TournamentBracket id="Bracket"/>
		<Panel id="NextOpponentContainer">
			<Panel id="NextOpponentCenterContainer" class="TournamentNodeSizeHuge">
				<Label id="RoundNameLabel" text="{s:tourney_round_name}" />
				<Label id="NextMatchLabel" text="Next Match" />
				<TournamentNodeTeam id="YourTeam" />
				<Label id="VsLabel" text="Versus" />
				<TournamentNodeTeam id="OpponentTeam" />
			</Panel>
		</Panel>
	</DOTALoadingScreenWeekendTourney>
</root>
