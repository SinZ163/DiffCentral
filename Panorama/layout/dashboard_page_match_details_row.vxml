<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_match_details.vcss_c" />
	</styles>

	<Panel class="PlayerRow">
		<Panel id="Player">
			<DOTAAvatarImage id="AvatarImage" />
			<Label class="PlayerName" text="{s:player_name}" />
		</Panel>
		<Label class="Level" text="{i:level}" />
		<Panel id="Hero">
			<DOTAHeroImage id="HeroIcon" heroimagestyle="landscape" scaling="stretch-to-fit-preserve-aspect" />
			<Label class="HeroName" text="{s:hero_name}" />
		</Panel>
		<Label class="Kills" text="{i:kills}" />
		<Label class="Deaths" text="{i:deaths}" />
		<Label class="Assists" text="{i:assists}" />
		<Image id="ItemIcon0" class="ItemIcon" />
		<Image id="ItemIcon1" class="ItemIcon" />
		<Image id="ItemIcon2" class="ItemIcon" />
		<Image id="ItemIcon3" class="ItemIcon" />
		<Image id="ItemIcon4" class="ItemIcon" />
		<Image id="ItemIcon5" class="ItemIcon" />
		<Label class="Gold" text="{i:gold}" />
		<Label class="LastHits" text="{i:last_hits}" />
		<Label class="Denies" text="{i:denies}" />
		<Label class="GoldPerMin" text="{i:gold_per_min}" />
		<Label class="XPPerMin" text="{i:xp_per_min}" />
		<Label class="MMRChange" text="+23" />
	</Panel>
</root>
