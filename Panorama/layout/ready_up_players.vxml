<root>
	<styles>
	<include src="s2r://panorama/styles/dotastyles.vcss_c" />
	<include src="s2r://panorama/styles/ready_up_players.vcss_c" />
	</styles>

	<DOTAReadyUpPlayers>

		<Panel id="PlayerReadyUp" class="TopBottomFlow StatusTextContainer">
			<Panel class="LeftRightFlow">
				<Panel id="PlayerSlotContainer" class="PlayerList">
					<!-- slots added by code -->
				</Panel>
			</Panel>
			<Label id="AcceptedCount" text="#dota_ready_up_num_accepted" />
		</Panel>

		<Panel id="ReturningToQueue" >
			<Label class="ReadyUpError" text="#returning_top_of_queue" />
		</Panel>

		<Panel id="ReturningToLobby">
			<Label class="ReadyUpError" text="#returning_to_lobby" />
		</Panel>

		<Panel id="PartyDeclined">
			<Label class="ReadyUpError" text="#ready_up_party_failed" />
		</Panel>

	</DOTAReadyUpPlayers>
</root>
