<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/fall2015_recycling.vcss_c" />
	</styles>

	<Panel class="RecyclingRoot">
		<Panel id="RecyclingInputs">
			<Label id="RecyclingInputsTitle" text="#DOTA_Recycling_DragItems" />
			<DOTACraftingInputGroup id="InputGroup" onfilled="SetPanelEnabled( CraftingCombineButton, true )" onnotfilled="SetPanelEnabled( CraftingCombineButton, false )">
				<Panel class="RecyclingInputRow">
					<DOTACraftingInputSlot slotnumber="1" recipeitemdef="11677" recipeslotid="0" />
					<DOTACraftingInputSlot slotnumber="2" recipeitemdef="11677" recipeslotid="0" />
					<DOTACraftingInputSlot slotnumber="3" recipeitemdef="11677" recipeslotid="0" />
					<DOTACraftingInputSlot slotnumber="4" recipeitemdef="11677" recipeslotid="0" />
					<DOTACraftingInputSlot slotnumber="5" recipeitemdef="11677" recipeslotid="0" />
				</Panel>
				<Panel class="RecyclingInputRow">
					<DOTACraftingInputSlot slotnumber="6" recipeitemdef="11677" recipeslotid="0" />
					<DOTACraftingInputSlot slotnumber="7" recipeitemdef="11677" recipeslotid="0" />
					<DOTACraftingInputSlot slotnumber="8" recipeitemdef="11677" recipeslotid="0" />
					<DOTACraftingInputSlot slotnumber="9" recipeitemdef="11677" recipeslotid="0" />
					<DOTACraftingInputSlot slotnumber="10" recipeitemdef="11677" recipeslotid="0" />
				</Panel>
			</DOTACraftingInputGroup>
		</Panel>
		<Panel id="RecyclingConvert">
			<Button id="CraftingCombineButton" onactivate="DOTAArmoryCraftingCombine(11677)" disabled="true">
				<Panel id="RecyclingArrow" />
				<Label text="#DOTA_Recycling_Action" />
			</Button>
		</Panel>
		<Panel id="RecyclingOutputs">
			<Label id="RecyclingOutputsTitle" text="#DOTA_Recycling_Outputs" />
			<Panel id="RecyclingOutputsContainer">
				<Panel id="RecyclingOutputsContents">
					<Panel id="RecyclingOutputImageContainer">
						<EconItemImage id="RecyclingOutputImage" itemdef="11353"  onmouseover="DOTAShowEconItemTooltip( 11353, 0, 0 );" onmouseout="DOTAHideEconItemTooltip();" />
					</Panel>
					<Label id="RecyclingOutputName" text="#DOTA_Fall2015Crafting_CharmName" />
					<Label id="RecyclingOutputBonus" text="#DOTA_Fall2015Crafting_CharmBonus" />
				</Panel>
			</Panel>
		</Panel>
	</Panel>
</root>
