				<Panel class="GlobalMainNavFooter">
					<Label class="GlobalMainNavCategory" text="#DOTA_GlobalItems_Interface"/>
				</Panel>
			</Panel>
			<Panel class="GlobalMainNav">
				<Image class="NavMenuImage" src="s2r://panorama/images/global_mainnav_audio_png.vtex" />
				<Panel class="GlobalCategorySubNavContainer">
					<Button class="GlobalCategorySubNav" onactivate="DOTAShowGlobalLoadoutPage( announcer )" >
						<Label text="#DOTA_GlobalItems_Announcers" />
					</Button>
          <Button class="GlobalCategorySubNav" onactivate="DOTAShowGlobalLoadoutPage( mega_kills )" >
            <Label text="#DOTA_GlobalItems_MegaKill" />
          </Button>
          <Button class="GlobalCategorySubNav" onactivate="DOTAShowGlobalLoadoutPage( music )" >
						<Label text="#DOTA_GlobalItems_Music" />
					</Button>
				</Panel>
				<Panel class="GlobalMainNavFooter">
					<Label class="GlobalMainNavCategory" text="#DOTA_GlobalItems_Audio"/>
				</Panel>
			</Panel>
