				<Panel class="GlobalCategorySubNavContainer">
					<Button class="GlobalCategorySubNav" onactivate="DOTAShowGlobalLoadoutPage( courier )">
						<Label text="#DOTA_GlobalItems_Couriers" />
					</Button>
<!-- 					<Button class="GlobalCategorySubNav" disabled="true">
						<Label text="#DOTA_GlobalItems_Pets" />
					</Button> -->
				</Panel>
				<Panel class="GlobalMainNavFooter">
					<Label class="GlobalMainNavCategory" text="#DOTA_GlobalItems_Companions"/>
				</Panel>
			</Panel>
			<Panel class="GlobalMainNav">
				<Image class="NavMenuImage" src="s2r://panorama/images/global_mainnav_world_png.vtex" />
				<Panel class="GlobalCategorySubNavContainer">
					<Button class="GlobalCategorySubNav" onactivate="DOTAShowGlobalLoadoutPage( terrain )" >
						<Label text="#DOTA_GlobalItems_Terrain" />
					</Button>					
					<Button class="GlobalCategorySubNav" onactivate="DOTAShowGlobalLoadoutPage( ward )">
