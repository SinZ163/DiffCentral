<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_econ_item_loadout.vcss_c" />
  </styles>

	<DOTALoadoutHeroStats>
		<Panel id="CompareButtonContainer">
			<TextButton id="CompareButton" text="Compare To All Heroes" onactivate="DOTAShowProfileHeroStatsPage()" />
		</Panel>
		<Panel class="HeroStatsRow">
			<Panel class="HeroStatsLeftBlock">
				<Panel class="BlockHeaderEmpty" />
				<Panel class="Block4Wide">
					<Panel class="HeroStatsSimpleStat">
						<Label class="HeroStatsSimpleStatValue" text="{s:games_played}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_Games" />
					</Panel>
					<Panel class="HeroStatsSimpleStat">
						<Label class="HeroStatsSimpleStatValue" text="{s:wins}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_Wins" />
					</Panel>
					<Panel class="HeroStatsSimpleStat">
						<Label class="HeroStatsSimpleStatValue" text="{s:losses}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_Losses" />
					</Panel>
					<Panel class="HeroStatsSimpleStat Wide">
						<Label class="HeroStatsSimpleStatValue" text="{s:win_rate}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_WinRate" />
					</Panel>
				</Panel>
			</Panel>
			<Panel class="HeroStatsRightBlock">
				<Label class="BlockHeader" text="Averages" />
				<Panel class="Block5Wide">
					<Panel class="HeroStatsSimpleStat">
						<Label class="HeroStatsSimpleStatValue" text="{s:avg_kills}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_Kills" />
					</Panel>
					<Panel class="HeroStatsSimpleStat">
						<Label class="HeroStatsSimpleStatValue" text="{s:avg_deaths}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_Deaths" />
					</Panel>
					<Panel class="HeroStatsSimpleStat">
						<Label class="HeroStatsSimpleStatValue" text="{s:avg_assists}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_Assists" />
					</Panel>
					<Panel class="HeroStatsSimpleStat Wide">
						<Label class="HeroStatsSimpleStatValue" text="{s:avg_gpm}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_GPM" />
					</Panel>
					<Panel class="HeroStatsSimpleStat Wide">
						<Label class="HeroStatsSimpleStatValue" text="{s:avg_xpm}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_XPM" />
					</Panel>
				</Panel>
			</Panel>
		</Panel>
		<Panel class="HeroStatsRow">
			<Panel class="HeroStatsLeftBlock">
				<Panel class="BlockHeaderEmpty" />
				<Panel class="BlockStatAndProgress">
					<Panel class="HeroStatsSimpleStat Wide">
						<Label class="HeroStatsSimpleStatValue" text="{s:current_win_streak}  ( {s:best_win_streak} )" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_StreakDesc" />
					</Panel>
					<Panel id="HeroStatsRatingBarContainer">
						<ProgressBar id="HeroStatsRatingBar" min="0.0" max="1.0" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_Rating" />
					</Panel>
				</Panel>
			</Panel>
			<Panel class="HeroStatsRightBlock">
				<Label class="BlockHeader" text="Best Records" />
				<Panel class="Block5Wide">
					<Panel class="HeroStatsSimpleStat">
						<Label class="HeroStatsSimpleStatValue" text="{s:best_kills}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_Kills" />
					</Panel>
					<Panel class="HeroStatsSimpleStat">
						<Label class="HeroStatsSimpleStatValue Disabled" text="--" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_Deaths" />
					</Panel>
					<Panel class="HeroStatsSimpleStat">
						<Label class="HeroStatsSimpleStatValue" text="{s:best_assists}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_Assists" />
					</Panel>
					<Panel class="HeroStatsSimpleStat Wide">
						<Label class="HeroStatsSimpleStatValue" text="{s:best_gpm}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_GPM" />
					</Panel>
					<Panel class="HeroStatsSimpleStat Wide">
						<Label class="HeroStatsSimpleStatValue" text="{s:best_xpm}" />
						<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_XPM" />
					</Panel>
				</Panel>
			</Panel>
		</Panel>
		<Panel class="HeroStatsRow">
			<Panel class="HeroStatsFullBlock">
				<Label class="HeroStatsSimpleStatName" text="#DOTA_HeroStat_MostRecentForHero" />
				<Panel class="HeroStatsHistoryContainer">
					<Panel class="HeroStatsHistoryContainerOverlap">
						<Panel class="HeroStatsHistoryBackgroundSlices">
							<!-- OnActivate set via code, or the Hidden class applied -->
							<Panel class="BackgroundSlice" id="BackgroundSlice0" />
							<Panel class="BackgroundSlice" id="BackgroundSlice1" />
							<Panel class="BackgroundSlice" id="BackgroundSlice2" />
							<Panel class="BackgroundSlice" id="BackgroundSlice3" />
							<Panel class="BackgroundSlice" id="BackgroundSlice4" />
							<Panel class="BackgroundSlice" id="BackgroundSlice5" />
							<Panel class="BackgroundSlice" id="BackgroundSlice6" />
							<Panel class="BackgroundSlice" id="BackgroundSlice7" />
							<Panel class="BackgroundSlice" id="BackgroundSlice8" />
							<Panel class="BackgroundSlice" id="BackgroundSlice9" />
							<Panel class="BackgroundSlice" id="BackgroundSlice10" />
							<Panel class="BackgroundSlice" id="BackgroundSlice11" />
							<Panel class="BackgroundSlice" id="BackgroundSlice12" />
							<Panel class="BackgroundSlice" id="BackgroundSlice13" />
							<Panel class="BackgroundSlice" id="BackgroundSlice14" />
							<Panel class="BackgroundSlice" id="BackgroundSlice15" />
							<Panel class="BackgroundSlice" id="BackgroundSlice16" />
							<Panel class="BackgroundSlice" id="BackgroundSlice17" />
							<Panel class="BackgroundSlice" id="BackgroundSlice18" />
							<Panel class="BackgroundSlice" id="BackgroundSlice19" />
						</Panel>
						<Panel class="HeroStatsHistoryContainerMain" hittest="false">
							<Panel id="HeroStatsHistoryWinsLosses" hittest="false">
								<!-- The following will all be either HistoryWin, HistoryLoss, or HistoryEmpty -->
								<Label id="HistoryWinLoss0" hittest="false" />
								<Label id="HistoryWinLoss1" hittest="false" />
								<Label id="HistoryWinLoss2" hittest="false" />
								<Label id="HistoryWinLoss3" hittest="false" />
								<Label id="HistoryWinLoss4" hittest="false" />
								<Label id="HistoryWinLoss5" hittest="false" />
								<Label id="HistoryWinLoss6" hittest="false" />
								<Label id="HistoryWinLoss7" hittest="false" />
								<Label id="HistoryWinLoss8" hittest="false" />
								<Label id="HistoryWinLoss9" hittest="false" />
								<Label id="HistoryWinLoss10" hittest="false" />
								<Label id="HistoryWinLoss11" hittest="false" />
								<Label id="HistoryWinLoss12" hittest="false" />
								<Label id="HistoryWinLoss13" hittest="false" />
								<Label id="HistoryWinLoss14" hittest="false" />
								<Label id="HistoryWinLoss15" hittest="false" />
								<Label id="HistoryWinLoss16" hittest="false" />
								<Label id="HistoryWinLoss17" hittest="false" />
								<Label id="HistoryWinLoss18" hittest="false" />
								<Label id="HistoryWinLoss19" hittest="false" />
							</Panel>
							<DOTACanvas id="HeroStatsHistoryCanvas" hittest="false" />
						</Panel>
					</Panel>
					<Panel class="HeroStatsHistoryContainerLegendContainer">
						<Panel class="HeroStatsHistoryContainerLegend">
							<Label class="LegendGPM" text="#DOTA_HeroStat_GPM" />
							<Label class="LegendXPM" text="#DOTA_HeroStat_XPM" />
							<Label class="LegendAssists" text="#DOTA_HeroStat_Assists" />
							<Label class="LegendKills" text="#DOTA_HeroStat_Kills" />
							<Label class="LegendDeaths" text="#DOTA_HeroStat_Deaths" />
						</Panel>
						<Panel class="HeroStatsHistoryContainerLegendTooltip">
							<Label id="TooltipBlockGameType" class="TooltipValue" text="#DOTA_HeroStat_GameType" />
							<Label id="TooltipBlockGameTypeRanked" class="TooltipValue" text="#DOTA_HeroStat_GameType_Ranked" />
							<Label id="TooltipBlockMatchDate" class="TooltipValue" text="{T:s:tooltip_match_date} {T:t:tooltip_match_date}" />
							<Panel class="TooltipBlockHR" />
							<Panel class="TooltipLTR">
								<Label id="TooltipWinLoss" />
								<Label id="TooltipWinLossValue" class="TooltipValue" text="{s:tooltip_won_text}" />
							</Panel>
							<Panel class="TooltipLTR" id="TooltipBlockGPM">
								<Panel id="TooltipSquareGPM" class="TooltipSquare" />
								<Label class="TooltipValue TooltipNumber MonoNumbersFont" text="{u:tooltip_gpm}" />
								<Label class="TooltipDesc" text="#DOTA_HeroStat_GPM" />
							</Panel>
							<Panel class="TooltipLTR" id="TooltipBlockXPM">
								<Panel id="TooltipSquareXPM" class="TooltipSquare" />
								<Label class="TooltipValue TooltipNumber MonoNumbersFont" text="{u:tooltip_xpm}" />
								<Label class="TooltipDesc" text="#DOTA_HeroStat_XPM" />
							</Panel>
							<Panel class="TooltipLTR">
								<Panel id="TooltipSquareAssists" class="TooltipSquare" />
								<Label class="TooltipValue TooltipNumber MonoNumbersFont" text="{u:tooltip_assists}" />
								<Label class="TooltipDesc" text="#DOTA_HeroStat_Assists" />
							</Panel>
							<Panel class="TooltipLTR">
								<Panel id="TooltipSquareKills" class="TooltipSquare" />
								<Label class="TooltipValue TooltipNumber MonoNumbersFont" text="{u:tooltip_kills}" />
								<Label class="TooltipDesc" text="#DOTA_HeroStat_Kills" />
							</Panel>
							<Panel class="TooltipLTR">
								<Panel id="TooltipSquareDeaths" class="TooltipSquare" />
								<Label class="TooltipValue TooltipNumber MonoNumbersFont" text="{u:tooltip_deaths}" />
								<Label class="TooltipDesc" text="#DOTA_HeroStat_Deaths" />
							</Panel>
						</Panel>
					</Panel>
				</Panel>
			</Panel>
		</Panel>
  </DOTALoadoutHeroStats>
	
</root>
