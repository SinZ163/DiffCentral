<root>
	<styles>
    <include src="s2r://panorama/styles/dotastyles.vcss_c" />
    <include src="s2r://panorama/styles/party.vcss_c" />
	</styles>

  <DOTAParty>

    <Panel id="PartyMembers" class="PlayerList">
		<DOTAGroupMemberSlot />
		<DOTAGroupMemberSlot />
		<DOTAGroupMemberSlot />
		<DOTAGroupMemberSlot />
		<DOTAGroupMemberSlot />
		<!-- Extra slot for a 6th player.  This is not a valid party to enter matchmaking,
		    but we allow such configurations temporarily to support coaching. -->
		<DOTAGroupMemberSlot class="HideEmptyGroupMembers"/>
	</Panel>

    <Panel id="CoachesSection" class="Hidden">
    	<Panel id="CoachSeparator" >
      		<Label text="#DOTA_PartyInvite_Coach"/>
    	</Panel>
      <Panel id="Coaches" class="PlayerList">
		<!-- Max 1 coach -->
        <DOTAGroupMemberSlot />
      </Panel>
    </Panel>

  </DOTAParty>
</root>
