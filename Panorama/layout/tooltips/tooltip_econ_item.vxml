<root>
  <styles>
    <include src="s2r://panorama/styles/dotastyles.vcss_c" />
    <include src="s2r://panorama/styles/tooltips/tooltip_econ_item.vcss_c" />
  </styles>

  <DOTATooltipEconItem class="EconItemContents">

	<Panel id="EconItemMain">
		<Panel id="EconTooltipHeader">
			<Panel id="HeaderTitle">
				<Label id="Name" text="#DOTA_tooltip_econ_item_name" />
				<Label id="Set" html="true" text="#DOTA_tooltip_econ_item_set" />
			</Panel>
			<Panel id="RarityStripe"/>
			<Panel id="Banner" >
				<Image id="HeroIcon"/>
				<Panel id="BannerText">
					<Panel class="LeftRightFlow">
						<Label text="#DOTA_tooltip_econ_item_rarity_label" /><Label id="Rarity" class="ItemRarityColor" text="#DOTA_tooltip_econ_item_rarity" />
					</Panel>
					<Label id="Slot" text="#DOTA_tooltip_econ_item_slot" />
				</Panel>
			</Panel>
		</Panel>
		<Panel id="AutographDetails">
			<Label id="AutographDetailsLabel"/>
		</Panel>
		<Panel id="Styles">
		  <Label id="StyleLabel" text="#DOTA_econ_item_details_style_label"/>
		  <Panel id="StylesList">
		  </Panel>
		</Panel>
		
		<Panel id="BrandPanel" class="EcontooltipGem LeftRightFlow">
			<Panel class="BrandIcon">
				<Image id="EconBrandIcon" src="s2r://panorama/images/econ/brands/brand_alliance_png.vtex" scaling="stretch-to-cover-preserve-aspect"/>
			</Panel>
			<Panel class="GemDetails TopBottomFlow">
				<Panel class="GemName GemBrand">
					<Label text="#DOTA_tooltip_econ_item_partner"/>	 
				</Panel>
				<Panel class="GemType GemBrand">
					<Label text=""/>	 
				</Panel>
			</Panel>
		</Panel>

		<Panel id="GemPanel" >
		</Panel>			
 				
		<Panel id="Tags">
			<Panel class="EconTooltipSectionTitle">
				<Label text="#DOTA_tooltip_econ_item_tags"/>
				<Panel class="SectionTitleBar"/>
			</Panel>
			<Panel id="TagsList">
			</Panel>
		</Panel>

		<Panel id="Modifiers">
			<Panel class="EconTooltipSectionTitle">
				<Label text="#DOTA_tooltip_econ_item_modifiers"/>
				<Panel class="SectionTitleBar"/>
			</Panel>
			<Panel id="ModifiersList" class="ListContainer">
				<Panel id="activity" class="ListedModifier">
					<Panel id="ModifierIcon_Animation" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_animation"/>
				</Panel>
				<Panel id="announcer" class="ListedModifier">
					<Panel id="ModifierIcon_Animation" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_announcer"/>
				</Panel>
				<Panel id="hud_skin" class="ListedModifier">
					<Panel id="ModifierIcon_Animation" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_HUD"/>
				</Panel>
				<Panel id="ability_name" class="ListedModifier">
					<Panel id="ModifierIcon_Animation" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_ability_name"/>
				</Panel>
				<Panel id="loading_screen" class="ListedModifier">
					<Panel id="ModifierIcon_Animation" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_loading_screen"/>
				</Panel>
				<Panel id="cursor_pack" class="ListedModifier">
					<Panel id="ModifierIcon_Animation" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_cursor_pack"/>
				</Panel>
				<Panel id="portrait_game" class="ListedModifier">
					<Panel id="ModifierIcon_Animation" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_portrait"/>
				</Panel>
				<Panel id="strange_control_point" class="ListedModifier">
					<Panel id="ModifierIcon_AmbientEffects" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_ambient_effects"/>
				</Panel>
				<Panel id="custom_kill_effect" class="ListedModifier">
					<Panel id="ModifierIcon_SpellEffects" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_kill_effects"/>
				</Panel>
				<Panel id="particle" class="ListedModifier">
					<Panel id="ModifierIcon_SpellEffects" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_spell_effects"/>
				</Panel>
				<Panel id="model" class="ListedModifier">
					<Panel id="ModifierIcon_AmbientEffects" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_model"/>
				</Panel>
				<Panel id="entity_scale" class="ListedModifier">
					<Panel id="ModifierIcon_AmbientEffects" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_scale"/>
				</Panel>
				<Panel id="additional_wearable" class="ListedModifier">
					<Panel id="ModifierIcon_AmbientEffects" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_wearable"/>
				</Panel>
				<Panel id="pet" class="ListedModifier">
					<Panel id="ModifierIcon_AmbientEffects" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_pet"/>
				</Panel>
				<Panel id="weather" class="ListedModifier">
					<Panel id="ModifierIcon_AmbientEffects" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_weather"/>
				</Panel>
				<Panel id="sound" class="ListedModifier">
					<Panel id="ModifierIcon_Voice" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_sound"/>
				</Panel>
				<Panel id="soundscape" class="ListedModifier">
					<Panel id="ModifierIcon_Voice" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_soundscape"/>
				</Panel>
				<Panel id="speech" class="ListedModifier">
					<Panel id="ModifierIcon_Voice" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_voice"/>
				</Panel>
				<Panel id="icon_replacement" class="ListedModifier">
					<Panel id="ModifierIcon_Icons" class="ModifierIcon"/>
					<Label text="#DOTA_tooltip_econ_item_modifier_icon"/>
				</Panel>
			</Panel>
		</Panel>
		<Panel id="Tournament" class="TopBottomFlow">
			<Panel class="EconTooltipSectionTitle">
				<Label text="#DOTA_tooltip_econ_item_tournament_drop"/>
				<Panel class="SectionTitleBar"/>
			</Panel>
			<Panel id="LeagueIDBox" class="LeftRightFlow">
				<Image id="LeagueLogo" scaling="stretch-to-fit-preserve-aspect" />
				<Panel class="TopBottomFlow">
					<Label id="LeagueName" text="#DOTA_tooltip_econ_item_leaguename"/>
					<Label id="LeagueEvent" text="#DOTA_tooltip_econ_item_matchevent"/>
				</Panel>
			</Panel>
			<Panel id="TeamsBox" class="LeftRightFlow">
				<Panel id="Team1" class="LeftRightFlow">
					<Image id="Team1Image" scaling="stretch-to-fit-preserve-aspect" />
					<Label id="Team1Name" text="#DOTA_tooltip_econ_item_team1_name"/>
				</Panel>
				<Label id="VsLabel" text="#DOTA_tooltip_econ_item_VS"/>
				<Panel id="Team2" class="LeftRightFlow">
					<Label id="Team2Name" text="#DOTA_tooltip_econ_item_team2_name"/>
					<Image id="Team2Image" scaling="stretch-to-fit-preserve-aspect" />
				</Panel>
			</Panel>
			<Label id="TournamentEvent" text="#DOTA_tooltip_econ_item_event_description" />
			<Panel id="TournamentDetails" class="LeftRightFlow">
				<Label id="TournamentMatchID" text="#DOTA_tooltip_econ_item_matchid" />
				<Label id="TournamentMatchDate" text="#DOTA_tooltip_econ_item_matchdate" />
			</Panel>
		</Panel>
		
		<Label id="Description" html="true" text="#DOTA_tooltip_econ_item_description" />

		<Panel id="Gift">
			<Panel class="EconTooltipSectionTitle LeftRightFlow">
				<Label text="#DOTA_econ_item_details_gift_label"/><Label id="GifterName" html="true" text="#DOTA_tooltip_econ_item_gifter"/>
				<Panel class="SectionTitleBar"/>
			</Panel>
			<Panel id="GiftDedication" class="TopBottomFlow">
				<Panel id="GiftDedicationBody">
					<Label id="GiftDedicationLabel"/>
				</Panel>
			</Panel>
			<Panel id="GiftReceived">
				<Label id="GiftReceivedDateLabel" text="#DOTA_tooltip_econ_item_gift_received_date"/>
			</Panel>
		</Panel>

		<Panel id="TemporaryRestrictions">
			<Panel id="TradeRestriction" class="RestrictionStrip">
				<Label id="TradeRestrictionLabel" />
			</Panel>
			<Panel id="MarketRestriction" class="RestrictionStrip">
				<Label text="#DOTA_tooltip_econ_item_market_restriction_permanent"/>
			</Panel>			
			<Panel id="GiftRestriction" class="RestrictionStrip">
				<Label id="GiftRestrictionLabel"/>
			</Panel>
		</Panel>
		<Panel id="DeletionRestriction" class="RestrictionStrip">
			<Label text="#DOTA_tooltip_econ_item_delete_restriction" />
		</Panel>
	
	</Panel>
	
	<Panel id="EconItemEquipped">
		<Image id="EquippedIcon" src="s2r://panorama/images/status_icons/econtooltip_equipped_psd.vtex" scaling="stretch-to-fit-preserve-aspect"/>
	</Panel>
	
	<Panel id="EconItemOverlayAutograph">
		<Image id="AutographImage" src="s2r://panorama/images/econ/autographs/281316113_png.vtex" scaling="stretch-to-fit-preserve-aspect"/>
	</Panel>
	
  </DOTATooltipEconItem>
</root>
