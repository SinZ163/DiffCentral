<root>
  <styles>
    <include src="s2r://panorama/styles/dotastyles.vcss_c" />
    <include src="s2r://panorama/styles/tooltips/tooltip_econ_item.vcss_c" />
  </styles>
  <Panel class="EcontooltipGem LeftRightFlow">		
    <Panel class="GemIconContainer">
	  <Panel class="GemIcon">
      <DOTAEconItemImage id="GemIconImage" src="s2r://panorama/images/econ/sockets/gem_stat_png.vtex" scaling="stretch-to-cover-preserve-aspect"/>
      <Image class="EmptyGemIconImage" src="s2r://panorama/images/econ/sockets/gem_stat_empty_png.vtex" scaling="stretch-to-cover-preserve-aspect"/>
      <Image id="GemIconOverlay" src="s2r://panorama/images/textures/gem_overlay_psd.vtex" scaling="stretch-to-cover-preserve-aspect"/>
	  </Panel>
	</Panel>
	<Panel class="GemDetails TopBottomFlow GemInscribed">
	  <Panel class="GemName LeftRightFlow">
	    <Label id="GemCount" text="#DOTA_tooltip_econ_item_gem_strange_count"/>
		<Label text="#DOTA_tooltip_econ_item_gem_description"/>
	  </Panel>
	  <Panel class="GemType">
	    <Label text="#DOTA_tooltip_econ_item_gem_type"/>
	  </Panel>
	</Panel>
  </Panel>
</root>