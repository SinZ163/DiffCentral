<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_shared.vcss_c" />
		<include src="s2r://panorama/styles/leaf_pages/dashboard_page_debut_styles.vcss_c" />
	</styles>

	<script>

		function checkPromoActive()
		{
			// Remove the PromoActive class after January 31st, 2015. The multiply by 1000 is because Date.now() is in milliseconds.
			var promoExpireTimestamp = 1454284800 * 1000;
			if ( Date.now() > promoExpireTimestamp )
			{
				$( '#MainContainer' ).RemoveClass( 'PromoActive' );
			}
		}

	</script>
	
	<DOTAReferencePage class="DashboardPage BGDark" onpagesetupsuccess="checkPromoActive()">

		<Panel id="MainContainer" class="ArcanaZeus PromoActive">
			<Panel id="TakeoverPageLeft" class="TakeOverPage" onactivate="">
				<Panel id="DebutBackgroundImage"/>
				<Panel id="ModelBackground"/>
				<DOTAScenePanel id="Model" map="backgrounds/hero_showcase_arcana_zeus" camera="hero_camera" light="hero_light" renderdeferred="false"/>
				<Panel id="BottomGradient"/>
				<Panel id="DebutInformation">
					<Panel id="InformationBody" class="TopBottomFlow">
						<Panel class="HeaderPanel TopBottomFlow">
							<Label id="ItemName" text="#Debut_Arcana_Zeus_Title"/>
							<Panel id="ItemSubtitle" class="LeftRightFlow">
								<Label id="Rarity" text="#DOTA_HeroLoadout_ArcanaFilterName"/>
								<Label id="" text="#Debut_Arcana_Zeus_Subtitle"/>
							</Panel>
						</Panel>
						<Label id="ItemLore" text="#Debut_Arcana_Zeus_Lore"/>
						<Panel id="InnerPanel" class="TopBottomFlow">
							<Label class="HeaderLabel" text="#Debut_Arcana_Zeus_Equip"/>
							<Panel class="BulletList LeftRightFlow">
								<Panel class="Bullet"/>
								<Label text="#Debut_Arcana_Zeus_Feature1"/>
							</Panel>
							<Movie class="VideoPanel" src="file://{resources}/videos/promo/feature1.webm" controls="none" repeat="true" autoplay="onload" />

							<Panel class="BulletList LeftRightFlow">
								<Panel class="Bullet"/>
								<Label text="#Debut_Arcana_Zeus_Feature2"/>
							</Panel>							
							<Movie class="VideoPanel" src="file://{resources}/videos/promo/feature2.webm" controls="none" repeat="true" autoplay="onload" />

							<Panel class="BulletList LeftRightFlow">
								<Panel class="Bullet"/>
								<Label text="#Debut_Arcana_Zeus_Feature3"/>
							</Panel>
							<Movie class="VideoPanel" src="file://{resources}/videos/promo/feature3.webm" controls="none" repeat="true" autoplay="onload" />

							<Panel class="BulletList LeftRightFlow">
								<Panel class="Bullet"/>
								<Label text="#Debut_Arcana_Zeus_Feature4"/>
							</Panel>
<!-- 							<Panel id="CustomStuffContainer" /> -->
							<Panel id="CustomContainer" class="LeftRightFlow">
								<Panel class="CustomSquare NoLeftMargin">
									<Panel id="Portrait" class="CustomImage"/>
								</Panel>
								<Panel class="CustomSquare">
									<Panel id="HeroIcon" class="CustomImage"/>
									<Panel id="MinimapIcon" class="CustomImage"/>
								</Panel>
								<Panel class="CustomSquare">
									<Panel id="AbilityIcon2" class="CustomImage"/>
								</Panel>
								<Panel class="CustomSquare">
									<Panel id="AbilityIcon" class="CustomImage"/>
								</Panel>
							</Panel>
							<Panel class="BulletList LeftRightFlow">
								<Panel class="Bullet"/>
								<Label text="#Debut_Arcana_Zeus_Feature5"/>
							</Panel>
							<Panel class="BulletList LeftRightFlow">
								<Panel class="Bullet"/>
								<Label text="#Debut_Arcana_Zeus_Feature6"/>
								<DOTAEmoticon id="Emoticon" alias="wrath" />
							</Panel>
							<Panel class="BulletList LeftRightFlow LastBullet">
								<Panel class="Bullet"/>
								<Label text="#Debut_Arcana_Zeus_Feature7"/>
							</Panel>							
						</Panel>
					</Panel>
					<Panel id="FooterPanel">
						<Panel id="Buttons" class="LeftRightFlow">
							<DemoModeButton class="SmallDemoButton PreviewMode" category="#DOTA_HeroLoadout_ArcanaFilterName" onactivate="DOTADemoHero( npc_dota_hero_zuus, 20844 )" />
              <Panel class="DemoSpacer" />
							<PurchaseButton itemdef="20844" onactivate="DOTAPurchaseItem( 20844, takeover )" />
						</Panel>			
					</Panel>					
					<Button class="CloseButton" onactivate="DOTANavigateBack()"/>
					<Panel class="Promo">
						<Label text="#Debut_Arcana_Zeus_Exalted"/>
					</Panel>
				</Panel>
			</Panel>
		</Panel>
	</DOTAReferencePage>
</root>

