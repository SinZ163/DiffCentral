<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard.vcss_c" />
		<include src="s2r://panorama/styles/tournament_node.vcss_c" />
		<include src="s2r://panorama/styles/tournament_node_team.vcss_c" />
	</styles>

	<TournamentNodeTeam>
		<Panel id="TeamLogoContainer">
			<DOTAUGCImage id="TeamLogo" />
			<DOTAAvatarImage id="UserAvatar" />
			<Panel class="Spinner" id="FindingOpponentSpinner" />
		</Panel>
		<Panel id="TeamNameContainer">
			<Label id="TeamName" text="{s:team_name}" />
			<DOTAUserName id="UserName" />
		</Panel>
		<Label id="TeamScore" class="MonoNumbersFont" text="{d:team_score}"/>
		
	</TournamentNodeTeam>
</root>

