<root>
	
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_shared.vcss_c" />
		<include src="s2r://panorama/styles/tournament_match_sidebar.vcss_c" />
	</styles>
	
	 <TournamentMatchSidebar>
		 <Panel id="Teams">
			 <DOTAUGCImage id="Team1Logo" class="TeamLogo" scaling="stretch-to-fit-preserve-aspect" />
			 <Label id="TBD1" text="#DOTA_TournamentBracket_MatchList_TBD"/>
			 <Label text="#vs"/>
			 <DOTAUGCImage id="Team2Logo" class="TeamLogo" scaling="stretch-to-fit-preserve-aspect" />
			 <Label id="TBD2" text="#DOTA_TournamentBracket_MatchList_TBD"/>
		</Panel>
		<Label id="UpcomingMatchStage" class="Centered" text="{s:stage_name}"/>
		<Label id="UpcomingMatchBestOf" class="Centered" text="{s:best_of}"/>
		<Label id="InProgress" class="Centered" text="#DOTA_TournamentBracket_Sidebar_Live"/>
		<Label id="GameNumber" class="Centered" text="#DOTA_TournamentBracket_Sidebar_GameNumber"/>
		<Label id="UpcomingMatchWinnerChampion" class="Centered Results" text="#DOTA_TournamentBracket_Sidebar_WinnerChampion"/>
		<Label id="UpcomingMatchWinnerGoes" class="Centered Results" text="#DOTA_TournamentBracket_Sidebar_WinnerGoes"/>
		<Label id="UpcomingMatchLoserGoes" class="Centered Results" text="#DOTA_TournamentBracket_Sidebar_LoserGoes"/>
		<Label id="UpcomingMatchLoserEliminated" class="Centered Results" text="#DOTA_TournamentBracket_Sidebar_LoserEliminated"/>
		<Label id="UpcomingMatchStatus" class="Centered" text="#DOTA_TournamentBracket_Sidebar_TimeRemaining"/>
		<Label id="UpcomingMatchSoon" class="Centered" text="#DOTA_TournamentBracket_Sidebar_Soon"/>
	
		<Button id="LiveMatchWatch" class="PopupButton LiveMatch Centered">
			<Label id="LiveMatchWatchLabel"   text="#DOTA_TournamentBracket_Sidebar_Watch"/>
		</Button>
	</TournamentMatchSidebar>

</root>
