<root>
  <styles>
    <include src="s2r://panorama/styles/dotastyles.vcss_c" />
    <include src="s2r://panorama/styles/ui_econ_set_preview.vcss_c" />
    <include src="s2r://panorama/styles/ui_econ_set_loadout_preview.vcss_c" />
  </styles>

  <DOTAUIEconSetPreview>
    <DOTAScenePanel id="Preview3DItems" antialias="true"/>
    <EconItemImage id="Preview2DItems" class="Rotating2dPanel" />
    <Image id="PreviewLoadingScreen" />
    <Panel id="PreviewSounds" >
      <Label class="PreviewSoundsTitle" text="#DOTA_PreviewSounds_ExampleSounds" />
      <Panel id="PreviewSoundList">
        <!-- Filled in dynamically -->
      </Panel>
    </Panel>
    <Panel class="PreviewEmoticonsContainer" >
      <Label class="PreviewEmoticonsTitle" text="#DOTA_PreviewEmoticons_Title" />
      <Panel id="PreviewEmoticons">
        <!-- Filled in dynamically -->
      </Panel>
    </Panel>
    <Button id="PreviewMusic" class="SoundPreviewItem" >
      <Label text="#DOTA_PreviewMusic" />
    </Button>
  </DOTAUIEconSetPreview>

</root>
