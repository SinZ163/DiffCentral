<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/lobby_player_row.vcss_c" />
	</styles>

	<DOTALobbyPlayerRow class="LobbyRow">

		<Panel class="LobbyCoachEmptyAvatar" />

		<DOTAAvatarImage class="LobbyPlayerImage" />
		<Label class="LobbyPlayerName" />

		<Button class="LobbyCoachJoinButton">
			<Label text="#DOTA_Workshop_Cursor_coach" />
		</Button>

		<Button class="LobbyPlayerLeaveButton" onactivate="DOTALobbyLeaveSlot()">
			<Label text="#dota_lobby_row_leave" />
		</Button>

		<Button class="LobbyPlayerKickButton" onactivate="DOTALobbyKickSlot()">
			<Label text="#DOTA_TeamKickLabel" />
		</Button>

	</DOTALobbyPlayerRow>

</root>
