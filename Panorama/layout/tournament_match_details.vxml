<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/tournament_match_details.vcss_c" />
	</styles>

	<TournamentMatchDetails acceptsfocus="true">
		<Panel class="TournamentMatchDetails">
			<Panel id="MatchDetailsVersus">
				<Panel id="MatchDetailsTeam1">
					<DOTAUGCImage id="MatchDetailsTeam1Logo" scaling="stretch-to-fit-preserve-aspect" />
					<Label class="UnknownTeamLabel" text="#DOTATournamentTeamUnknown" />
				</Panel>

				<Label id="MatchDetailsVsLabel" text="#DOTATournamentTeamVersus" />
				
				<Panel id="MatchDetailsTeam2">
					<DOTAUGCImage id="MatchDetailsTeam2Logo" scaling="stretch-to-fit-preserve-aspect" />
					<Label class="UnknownTeamLabel" text="#DOTATournamentTeamUnknown" />
				</Panel>
			</Panel>
			<Label id="StageName" text="{s:stage_name}" />
			<Label id="BestOf" text="{s:best_of}" />
			<Panel id="MatchDetailsGames">

			</Panel>
			<Panel id="MatchDetailsGeneralInfo">
				<Label id="UpcomingMatchWinnerChampion" class="Centered Results" text="#DOTA_TournamentBracket_Sidebar_WinnerChampion"/>
				<Label id="UpcomingMatchWinnerGoes" class="Centered Results" text="#DOTA_TournamentBracket_Sidebar_WinnerGoes"/>
				<Label id="UpcomingMatchLoserGoes" class="Centered Results" text="#DOTA_TournamentBracket_Sidebar_LoserGoes"/>
				<Label id="UpcomingMatchLoserEliminated" class="Centered Results" text="#DOTA_TournamentBracket_Sidebar_LoserEliminated"/>
			</Panel>
		</Panel>
	</TournamentMatchDetails>
</root>
