<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_owned_item_details_item.vcss_c" />
    <include src="s2r://panorama/styles/tooltips/tooltip_econ_item.vcss_c" />
	</styles>
	<Panel class="Root">
    <Panel id="ItemEdit">
      <Panel id="ItemEditCharges">
        <Label text="12 Item Modification Charges Available"/>
        <Panel class="InfoIcon"/>
      </Panel>
      <Panel id="ItemEditBackground">
        <Panel id="ItemEditContainer" class="TopBottomFlow" >
          <Panel id="EconTooltipHeader" class="ItemRarityBackgroundColor ItemRarity_rare">
             <Panel class="EconItemTitleBar">
               <Panel class="BrandPanel">
					      <Label class="EconBrandName" text="#DOTA_tooltip_econ_item_partner"/>	 
					      <Image id="EconBrandIcon" scaling="stretch-to-cover-preserve-aspect"/>
			        </Panel>
              <Panel class="ItemHeader" >               
                <DOTAEconItem id="EditedItemIcon"/>
                <Panel id="ItemNameSection" class="TopBottomFlow">
                  <Panel class="LeftRightFlow" >
                    <Panel class="NameHeader LeftRightFlow">
                      <Label id="OriginalName" text="#DOTA_econ_item_details_original_name_label" />
                      <Label id="Name" text="#DOTA_econ_item_details_original_item_name" />
                    </Panel>
                    <Button class="NameResetButton ControlIconButton EditButton" onactivate="DOTAItemResetName()"  onmouseover="DOTAShowTextTooltip( #DOTA_econ_item_details_reset_name )" onmouseout="DOTAHideTextTooltip()"/>
                  </Panel>
                  <Panel class="LeftRightFlow">
                    <Label class="ItemName" text="#DOTA_econ_item_details_item_name" />
                    <Panel class="InfoIcon" onmouseover="DOTAShowTextTooltip( #DOTA_ItemNameChange_Tooltip )" onmouseout="DOTAHideTextTooltip()"/>
                    <Button class="NameChangeButton ControlIconButton EditButton" onactivate="DOTAItemChangeName()"  onmouseover="DOTAShowTextTooltip( #DOTA_econ_item_details_edit_name )" onmouseout="DOTAHideTextTooltip()"/>
                  </Panel>
                  <Label id="Set" html="true" text="#DOTA_tooltip_econ_item_set" />
                </Panel>
              </Panel>
              <Button class="CloseButton" onactivate="DOTAOwnedItemDetailsExit()" />
            </Panel>
            <Panel id="RarityStripe"/>
              <Panel id="Banner" >
                <DOTAHeroImage id="HeroIcon" heroimagestyle="landscape"/>
                <Panel id="BannerText">
                  <Panel class="LeftRightFlow">
                    <Label text="#DOTA_tooltip_econ_item_rarity_label" />
                    <Label id="Rarity" class="ItemRarityColor" text="#DOTA_tooltip_econ_item_rarity" />
                  </Panel>
                  <Label id="Slot" text="#DOTA_tooltip_econ_item_slot" />
                </Panel>
                <Panel id="AutographDetails">
    		   	      <Label id="AutographDetailsLabel"/>
		    	      </Panel>
            </Panel>
          </Panel>
          <Panel id="ItemEditBody" class="TopDownFlow">
            <Panel id="EquipStatePanel" class="EditSection TopBottomFlow">
              <Panel class="EditSectionHeader LeftRightFlow">
                <Label class="HeaderText" text="#DOTA_econ_item_details_equip_header"/>
                <Button id="ToggleEquipButton" class="ButtonBevel" onactivate="DOTAItemToggleEquipState()" >
                  <Label text="#DOTA_econ_item_details_toggle_equip_state"/>
                </Button>
              </Panel>
            </Panel>
            <Panel class="PurgatoryPanel EditSection TopBottomFlow">
              <Panel class="EditSectionHeader LeftRightFlow">
                <Panel class="PurgatoryHeader LeftRightFlow">
                  <Label class="HeaderText" text="#DOTA_econ_item_details_purgatory_header"/>
                  <Panel class="InfoIcon" onmouseover="DOTAShowTextTooltip( #DOTA_Purgatory_Tooltip )" onmouseout="DOTAHideTextTooltip()"/>
                </Panel>
                <Panel class="DoubleButton">
                  <Button class="ButtonBevel" onactivate="DOTAItemPurgatoryRefund()" >
                    <Label text="#DOTA_econ_item_details_purgatory_refund"/>
                  </Button>
                  <Button class="ButtonBevel" onactivate="DOTAItemPurgatoryFinalize()" >
                    <Label text="#DOTA_econ_item_details_purgatory_finalize"/>
                  </Button>
                </Panel>
              </Panel>
            </Panel>
            <Panel id="RareSocketPanel" class="EditSection TopBottomFlow">
              <Panel class="EditSectionHeader LeftRightFlow">
                <Label class="HeaderText" text="#DOTA_econ_item_details_rare_sockets_header"/>
                <Panel class="InfoIcon" onmouseover="DOTAShowTextTooltip( #DOTA_RareSocketing_Tooltip )" onmouseout="DOTAHideTextTooltip()"/>
              </Panel>
              <Panel id="RareSocketList" >
                <!-- Filled in dynamically -->
              </Panel>
              <Panel class="EditSectionFooter">
                <Label class="FooterText" text="#DOTA_econ_item_details_available_rare_gem_count"/>
                <Panel class="InfoIcon"  onmouseover="DOTAShowTextTooltip( #DOTA_Socketing_ExtractRareGems_Tooltip )" onmouseout="DOTAHideTextTooltip()"/>
                <Button class="RemoveRareGemsButton ButtonBevel" onactivate="DOTAItemExtractRareGems()" >
                  <Label text="#DOTA_econ_item_details_extract_rare_gems"/>
                </Button>
              </Panel>
            </Panel>
            <Panel id="SocketPanel" class="EditSection TopBottomFlow">
             <Panel class="EditSectionHeader LeftRightFlow">
              <Label class="HeaderText" text="#DOTA_econ_item_details_sockets_header"/>
              <Panel class="InfoIcon" onmouseover="DOTAShowTextTooltip( #DOTA_Socketing_Tooltip )" onmouseout="DOTAHideTextTooltip()"/>
             </Panel>
              <Panel id="SocketList" >
                <!-- Filled in dynamically -->
              </Panel>
              <Panel class="EditSectionFooter">
                <Label class="FooterText" text="#DOTA_econ_item_details_available_gem_count"/>
                <Panel class="InfoIcon AddSocketInfo" onmouseover="DOTAShowTextTooltip( #DOTA_Socketing_AddSocket_Tooltip )" onmouseout="DOTAHideTextTooltip()"/>
                <Button class="AddSocketButton ButtonBevel" onactivate="DOTAItemAddSocket()" >
                  <Label text="#DOTA_econ_item_details_add_socket"/>
                </Button>              
              </Panel>
            </Panel>

            <Panel id="GiftPanel" class="EditSection TopBottomFlow">
              <Panel class="EditSectionHeader">
                <Label class="HeaderText" text="#DOTA_econ_item_details_gift_header"/>
                <Label class="GiftDate" text="#DOTA_tooltip_econ_item_gift_received_date"/>
              </Panel>
              <Panel class="EditRow">
                <Panel class="GifterInfo">
                  <Label class="GiftFrom" text="#DOTA_econ_item_details_gift_label" />                  
                  <Label class="GifterName" text="#DOTA_tooltip_econ_item_gifter" html="true" />
                </Panel>                
              </Panel>
              <Panel class="EditRow" >
                <Panel id="GiftDedicationBody">
                  <Label text="#DOTA_tooltip_econ_item_gift_dedication"/>
                </Panel>
                <Button class="ButtonBevel" onactivate="DOTAItemResetGifterAttributes()" >
                  <Label text="#DOTA_econ_item_details_gift_clear_attributes"/>
                </Button>
              </Panel>
            </Panel>

            <Panel class="EditSection TopBottomFlow StylesPanel">
              <Panel class="EditSectionHeader">
                <Label class="HeaderText" text="#DOTA_econ_item_details_styles_header"/>
              </Panel>
              <Panel id="StylesList">
                <!-- Filled in dynamically -->
              </Panel>
            </Panel>
            
            <Panel class="EditSection TopBottomFlow">
              <Panel class="EditSectionHeader LeftRightFlow">
                <Label class="HeaderText" text="#DOTA_econ_item_details_description_header"/>
                <Panel class="InfoIcon" onmouseover="DOTAShowTextTooltip( #DOTA_ItemDescChange_Tooltip )" onmouseout="DOTAHideTextTooltip()"/>
              </Panel>
              <Panel class="EditRow">
                <Label class="Description" html="true" text="#DOTA_econ_item_details_item_description"/>
              </Panel>
              <Panel class="EditSectionFooter">
                <Panel class="DoubleButton">
                  <Button class="ResetDescription ButtonBevel HalfSizeButton ModCharge" onactivate="DOTAItemResetDescription()">
                    <Label text="#DOTA_econ_item_details_description_reset"/>
                  </Button>
                  <Button class="ButtonBevel HalfSizeButton ModCharge" onactivate="DOTAItemChangeDescription()">
                    <Label text="#DOTA_econ_item_details_description_edit"/>
                  </Button>
                </Panel>
              </Panel>
            </Panel>
          </Panel>

          <Panel id="ItemEditFooter">
            <Panel class="FooterButtonContainer" >
             <Button class="FooterButton ButtonBevel" onactivate="DOTAOwnedItemDetailsGoToLoadout()">
                <Label text="#DOTA_econ_item_details_go_to_loadout"/>
              </Button>
            </Panel>
          </Panel>
        </Panel>
        <Panel id="EconItemOverlayAutograph">
          <Image id="AutographImage" src="s2r://panorama/images/econ/autographs/281316113_png.vtex" scaling="stretch-to-fit-preserve-aspect"/>
        </Panel>
      </Panel>
    </Panel>
  </Panel>
