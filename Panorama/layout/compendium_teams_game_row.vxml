<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/winter2016/comp_shanghai2016_teams.vcss_c" />
	</styles>

	<DOTACompendiumTeamsGameRow>
		<Label class="DateColumn" text="{t:s:timestamp}"/>
		<Label class="TimeColumn" text="{t:t:timestamp}"/>
		<Label class="ResultColumn" text="{s:result}" html="true"/>
		<Label class="TournamentColumn" text="{s:tournament}"/>
		<Label class="DurationColumn" text="{t:d:duration}"/>
		<Label class="TypeColumn" text="#DOTA_CompendiumTeam_Game_SeriesGame"/>
		<Panel class="ButtonsColumn LeftRightFlow">
			<Button id="Details" class="FlatBlueButton RightAlign">
				<Label text="#DOTA_CompendiumTeam_Game_Details"/>
			</Button>
		</Panel>
	</DOTACompendiumTeamsGameRow>
</root>
