<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/dota_hud_ingame_prediction_result.vcss_c" />
	</styles>

	<DOTAIngamePredictionResult>

		<Panel class="ResultTopRow" >
			<Panel class="NoResult">
				<Label text="#DOTA_InGamePrediction_NoResult" />
			</Panel>

			<Panel class="TeamResult">
				<DOTAUGCImage id="CustomTeamLogo" class="LogoCustom" scaling="stretch-to-fit-preserve-aspect" />
				<Panel class="LogoDefault">
					<Panel class="Radiant" scaling="stretch-to-fit-preserve-aspect" />
					<Panel class="Dire" scaling="stretch-to-fit-preserve-aspect" />
				</Panel>
				<Label class="TeamName" text="{s:team_name}" />
				<Label class="RawValue" text="({s:raw_value})" />
			</Panel>

			<Panel class="PlayerResult">
				<DOTAHeroImage id="HeroIcon" heroimagestyle="landscape" scaling="stretch-to-fit-preserve-aspect" />
				<Label class="HeroName" text="{s:hero_name}" />
				<Label class="PlayerName" text="{s:player_name}" />
				<Label class="RawValue" text="({s:raw_value})" />
			</Panel>

			<Panel class="TextResult">
				<Label class="TextValue" text="{s:generic_result_text}" />
				<Label class="RawValue" text="({s:raw_value})" />
			</Panel>
			
		</Panel>

	</DOTAIngamePredictionResult>
</root>


