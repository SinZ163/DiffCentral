<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_shared.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/spring2016/comp_manila2016_pages.vcss_c" />
	</styles>

	<script>
	</script>

	<DOTACompManila2016PlayerCardDetailsPage class="DashboardPage" hittest="false">
		<Panel id="CompendiumBody" class="CompendiumBody">
			<Panel id="PlayerCardDetailsContainer">
				<Carousel id="CardCarousel" class="SingleItemCarousel" panels-visible="1">
				<!-- dynamically filled with cards -->
				</Carousel>
				<CarouselNav class="TransportControls" carouselid="CardCarousel" maxpips="20" />
				<Button id="RecycleButton" class="ButtonBevel" onactivate="DOTARecyclePlayerCard()"><Label text="#DOTA_PlayerCardsRecycle"/></Button>
				<Label id="DustCount" text="#DOTA_PlayerCardsDustCountCurrent"/>
			</Panel>
			<Button id="BackButton" class="ButtonBevel" onactivate="DOTAShowCompManila2016PlayerCardsPage()">
				<Label text="#DOTA_PlayerCard_Return"/>
			</Button>
		</Panel>		
	</DOTACompManila2016PlayerCardDetailsPage>
</root>
