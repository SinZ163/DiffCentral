<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_shared.vcss_c" />
   		<include src="s2r://panorama/styles/compendiums/compendium_styles.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/spring2016/bp_spring2016.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/spring2016/bp_spring2016_community_goals.vcss_c" />
	</styles>

	<script>
	</script>

	<DOTABPSpring2016CommunityGoalsPage class="DashboardPage">
		<Panel id="CommunityChallengeBackground">
			<Panel id="CompendiumBody" class="CompendiumBody">
				<Panel id="Container" >
					<Panel id="LeftPanel">
						
					</Panel>
					<Panel id="CenterPanel">
						<Label class="Title" text="#DOTA_Spring2016_Community_Challenge_PageHeader" />
						<Panel id="ChallengePanel">
							<Label id="CurrentChallengeLabel" text="#DOTA_Spring2016_Community_Challenge_GoalNow"/>
							<Label id="CurrentChallengeName" text="#DOTA_Spring2016_Community_Challenge_GoalTitle"/>
							<Label id="CurrentChallengeDescription" text="#DOTA_Spring2016_Community_Challenge_GoalText"/>
							<Panel id="CurrentChallengeProgressPanel">
								<ProgressBar id="CurrentChallengeProgressBar" min="0" max="10000" value="5000"/>
								<Panel id="CurrentChallengeProgressLabelsContainer">
									<Label id="CurrentChallengeProgressMin" text="{d:goal_progress}" />
									<Label id="CurrentChallengeProgressMax" text="{d:goal_target_value}" />
								</Panel>
							</Panel>
						</Panel>
						<Panel id="OverviewPanel" >
							<Label class="Heading" text="#DOTA_Spring2016_Community_Challenge_GoalBox"/>
							
							<Panel id="ChallengePathContainer">
								<Panel id="Goal1" class="Goal" GoalID="1"/>
								<Panel class="GoalSpace" />
								<Panel id="Goal2" class="Goal" GoalID="2"/>
								<Panel class="GoalSpace" />
								<Panel id="Goal3" class="Goal" GoalID="3"/>
								<Panel class="GoalSpace" />
								<Panel id="Goal4" class="Goal" GoalID="4"/>
								<Panel class="GoalSpace" />
								<Panel id="Goal5" class="Goal" GoalID="5"/>
								<Panel class="GoalSpace" />
								<Panel id="Goal6" class="Goal" GoalID="6"/>
							</Panel>					
						</Panel>
					</Panel>
					<Panel id="RightPanel">
						<Label class="Heading" text="#DOTA_Spring2016_Community_Challenge_IntroTitle"/>
						<Label class="Body" text="#DOTA_Spring2016_Community_Challenge_IntroText"/>
						<Label class="Heading" text="#DOTA_Spring2016_Community_Challenge_RewardsTitle"/>
						<Label class="Body" text="#DOTA_Spring2016_Community_Challenge_RewardsText"/>
						<Panel id="RightImage"/>
						<Panel id="PlaysRemainingContainer">
							<Label id="PlaysRemainingLabelButton" text="#DOTA_Spring2016_PachinkoInferno_BallsRemain"/>
							<Button id="PlayLinaGameButton" class="ButtonBevel" onactivate="DOTAPlayLinaGame()"><Label text="#DOTA_Spring2016_PachinkoInferno_PlayButton"/></Button>
						</Panel>
					</Panel>				
				</Panel>
			</Panel>
		</Panel>
	</DOTABPSpring2016CommunityGoalsPage>
</root>
