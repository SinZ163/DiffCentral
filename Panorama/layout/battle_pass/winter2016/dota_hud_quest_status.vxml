<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/winter2016/dota_hud_quest_status.vcss_c" />
	</styles>

	<DOTAQuestStatus class="QuestStatusHidden" hittest="false">
		<Panel id="QuestStatusContainer" hittest="false" class="TopBottomFlow">
			<Panel id="Quest1Status" class="QuestStatusPanel Quest1" hittest="false">
				<Panel class="QuestListItem LeftRightFlow" hittest="false">
					<Button id="QuestExpandButton1" class="QuestExpandButton" onactivate="ToggleStyle( QuestStatusContainer, Expand) ">
						<Panel class="CheckIcon QuestStatusIcon"/>
						<Panel class="XIcon QuestStatusIcon"/>
					</Button>
					<Label id="Progress1" class="ProgressNumbers" text="{d:progress1} / {d:goal1}" hittest="false"/>
					<Panel class="QuestDesc LeftRightFlow" hittest="false">
						<Label id="Quest1" text="Quest1" html="true" hittest="false"/>
					</Panel>
				</Panel>
			</Panel>
			
			<Panel id="Quest2Status" class="QuestStatusPanel QuestExpandSecondary Quest2 HiddenQuest" hittest="false">
				<Panel class="QuestListItem LeftRightFlow" hittest="false">
					<Button id="QuestExpandButton2" class="QuestExpandButton Success" onactivate="ToggleStyle( QuestStatusContainer, Expand) ">
						<Panel class="CheckIcon QuestStatusIcon"/>
						<Panel class="XIcon QuestStatusIcon"/>						
					</Button>
					<Label id="Progress2" class="ProgressNumbers" text="{d:progress2} / {d:goal2}" hittest="false"/>		
					<Panel class="QuestDesc LeftRightFlow" hittest="false">
						<Label id="Quest2" text="Quest2" html="true" hittest="false"/>
					</Panel>
				</Panel>
			</Panel>

			<Panel id="Quest3Status" class="QuestStatusPanel QuestExpandSecondary Quest3 HiddenQuest" hittest="false">
				<Panel class="QuestListItem LeftRightFlow" hittest="false">
					<Button id="QuestExpandButton3" class="QuestExpandButton Success" onactivate="ToggleStyle( QuestStatusContainer, Expand) ">
						<Panel class="CheckIcon QuestStatusIcon"/>
						<Panel class="XIcon QuestStatusIcon"/>						
					</Button>
					<Label id="Progress3" class="ProgressNumbers" text="{d:progress3} / {d:goal3}" hittest="false"/>
					<Panel class="QuestDesc LeftRightFlow" hittest="false">
						<Label id="Quest3" text="Quest3" html="true" hittest="false"/>
					</Panel>
				</Panel>
			</Panel>
		</Panel>
	</DOTAQuestStatus>
</root>


