<root>
	<styles>
    <include src="s2r://panorama/styles/gamestyles.vcss_c" />
    <include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
	<include src="s2r://panorama/styles/battle_pass/spring2016/bp_spring2016.vcss_c" />		
    <include src="s2r://panorama/styles/popups/popup_ui_battle_pass_log.vcss_c" />
    <include src="s2r://panorama/styles/popups/popup_ui_battle_pass_log_entry.vcss_c" />
  </styles>

  <DOTAPopupUIBattlePassLog class="PopupPanel Hidden" popupbackground="dim" >
    
    <Panel id="Title">
		<Panel id="TitleBackground">
		<Button id="CancelButton" class="CloseButton" onactivate="UIPopupButtonClicked()" />		
		<Panel id="LevelWell">
			<Panel id="LevelCircle">
				<Label class="TotalLevel" text="#DOTA_BattlePassLog_TotalLevel" />
			</Panel>
		</Panel>
        <Label class="TitleLabel" text="#DOTA_BattlePassLog_Title" />
		<Label class="TitleSubLabel TotalPoints" text="#DOTA_BattlePassLog_TotalPoints" />
        <Panel class="TitleSection">
          
        </Panel>        
      </Panel>
    </Panel>
    <Panel class="TitleBar">
      <Label class="LevelColumn" text="#DOTA_BattlePassLog_TitleBar_BattleLevel"/>
	  <Label class="DateColumn" text="#DOTA_BattlePassLog_TitleBar_Date"/>	  
	  <Label class="DescriptionColumn" text="#DOTA_BattlePassLog_TitleBar_Description"/>     
	  <Panel class="LeftRightFlow">
		<Panel class="PointTotalIcon"/>
		<Label class="PointsColumn" text="#DOTA_BattlePassLog_TitleBar_Points"/>
	  </Panel>
	  
	  <Label class="TotalPointsColumn" text="#DOTA_BattlePassLog_TitleBar_TotalPoints"/>      
     
    </Panel>
	<Panel id="Body">
		<!-- Filled in dynamically -->    
	</Panel>
  
  </DOTAPopupUIBattlePassLog>
</root>
