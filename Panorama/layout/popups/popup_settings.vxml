					<DOTASettingsKeyBinder text="#dota_chatwheel_label_GetBack" bind="ChatWheelGetBack" />
					<DOTASettingsKeyBinder text="#dota_chatwheel_label_NeedWards" bind="ChatWheelNeedWards" />
