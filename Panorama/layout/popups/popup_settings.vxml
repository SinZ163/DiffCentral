								<Panel class="ControlGroupRow">
									<DOTASettingsKeyBinder text="#dota_settings_group1" bind="ControlGroup1" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_Group, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
									<DOTASettingsKeyBinder text="#dota_settings_group2" bind="ControlGroup2" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_Group, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
								</Panel>
								<Panel class="ControlGroupRow">
									<DOTASettingsKeyBinder text="#dota_settings_group3" bind="ControlGroup3" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_Group, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
									<DOTASettingsKeyBinder text="#dota_settings_group4" bind="ControlGroup4" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_Group, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
								</Panel>
								<Panel class="ControlGroupRow">
									<DOTASettingsKeyBinder text="#dota_settings_group5" bind="ControlGroup5" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_Group, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
									<DOTASettingsKeyBinder text="#dota_settings_group6" bind="ControlGroup6" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_Group, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
