							<DOTASettingsKeyBinder text="#dota_settings_controlgroupcycle" bind="ControlGroupCycle" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_ControlGroupCycle, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
