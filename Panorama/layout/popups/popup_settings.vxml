<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_settings.vcss_c" />
	</styles>

	<PopupSettings class="Hidden" defaultfocus="TestContainer" hittest="false" popupbackground="blur" oncancel="DOTASettings();" >

		<!-- Nav Bar -->
		<Panel id="SettingsNavBar">
			<Button id="DOTACloseSettingsButton" class="ControlIconButton" onactivate="DOTASettings();" />
			<RadioButton id="HotkeysTabButton" class="SettingsNavBarButton" group="SettingsTopMenu" onactivate="DOTASetActiveTab(0);">
				<Label text="#dota_settings_hotkeys" />
			</RadioButton>
			<Label class="SettingsTabSeparator" text="/" />
			<RadioButton id="OptionsTabButton"	class="SettingsNavBarButton" group="SettingsTopMenu" onactivate="DOTASetActiveTab(1);">
				<Label text="#dota_settings_options"/>
			</RadioButton>
			<Label class="SettingsTabSeparator"	text="/" />
			<RadioButton id="VideoTabButton" class="SettingsNavBarButton" group="SettingsTopMenu" onactivate="DOTASetActiveTab(2);" >
				<Label text="#dota_settings_video" />
			</RadioButton>
			<Label class="SettingsTabSeparator" text="/" />
			<RadioButton id="AudioTabButton"	class="SettingsNavBarButton" group="SettingsTopMenu" onactivate="DOTASetActiveTab(3);" >
				<Label text="#dota_settings_audio" />
			</RadioButton>
			<Panel class="SettingsTabFiller" />
			<RadioButton id="AboutTabButton"	class="SettingsNavBarButton" group="SettingsTopMenu" onactivate="DOTASetActiveTab(4);" >
				<Label text="#dota_settings_about" />
			</RadioButton>
		</Panel>

		<!-- Hotkeys -->
		<Panel id="HotkeysTabContent" class="SettingsSecondaryPanel" >
			<Panel class="HotKeyMainTab" id="InGameSubTab">
				<Panel class="SettingsColumnContainer">
					<Panel id="Column0" class="HotKeySettingsColumn">
						<Panel id="Abilities" class="SettingsSection">
							<Panel class="SectionHeaderContainer">
								<Label text="#dota_settings_abilities" class="SectionHeader" />
								<ToggleButton class="CheckBox" id="LegacyKeysCheckBox" text="#dota_settings_legacykeys"	onactivate="DOTASetLegacyKeys();" onmouseover="DOTAShowTitleTextTooltip( #dota_settings_legacykeys, #DOTA_Hotkeys_Tooltip_Legacy)" onmouseout="DOTAHideTitleTextTooltip()" />
							</Panel>
							<Panel class="SectionHeaderLineNoMargin"/>

							<Panel id="AbilitiesContents">
								<Panel id="AbilitiesActive">
									<Panel id="AbilityBindingTabBar">
										<RadioButton id="HeroAbilityTabButton" group="AbilityBindingUnitType" class="AbilityNavBarButton" text="#dota_settings_abilities_hero" onactivate="DOTASetActiveAbilitySubTab(0);" />
										<RadioButton id="NeutralAbilityTabButton" group="AbilityBindingUnitType" class="AbilityNavBarButton" text="#dota_settings_abilities_neutrals" onactivate="DOTASetActiveAbilitySubTab(1);" />
										<RadioButton id="CourierAbilityTabButton" group="AbilityBindingUnitType" class="AbilityNavBarButton" text="#dota_settings_abilities_courier" onactivate="DOTASetActiveAbilitySubTab(2);" />
									</Panel>
									<Panel id="AbilityBindingContents">

										<Panel id="AbilityBindings_Hero" >
										<!-- SPELL ICONS -->											
											<Panel id="AbilityBoxesBG">
	 	 										<Panel id="HotkeysAbilityIcon1" class="HeroAbilityIcon" >
	 	 											<Label text="#dota_settings_abilities_primary1"/>
	 	 										</Panel>
	 	 										<Panel id="HotkeysAbilityIcon2" class="HeroAbilityIcon" >
	 	 											<Label text="#dota_settings_abilities_primary2"/>
	 	 										</Panel>
	 	 										<Panel id="HotkeysAbilityIcon3" class="HeroAbilityIcon" >
	 	 											<Label text="#dota_settings_abilities_primary3"/>
	 	 										</Panel>
	 	 										<Panel id="HotkeysAbilityIcon4" class="HeroAbilityIcon" >
	 	 											<Label text="#dota_settings_abilities_secondary1"/>
	 	 										</Panel>
	 	 										<Panel id="HotkeysAbilityIcon5" class="HeroAbilityIcon" >
	 	 											<Label text="#dota_settings_abilities_secondary2"/>
	 	 										</Panel>
	 	 										<Panel id="HotkeysAbilityIcon6" class="HeroAbilityIcon" >
	 	 											<Label text="#dota_settings_abilities_ultimate"/>
	 	 										</Panel>
											</Panel>
										<!-- CAST -->											
											<Panel id="AbilityBoxesCast">
												<DOTASettingsKeyBinder bind="AbilityPrimary1" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder bind="AbilityPrimary2" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder bind="AbilityPrimary3" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder bind="AbilitySecondary1" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder bind="AbilitySecondary2" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder bind="AbilityUltimate" class="HeroAbilityBindAbilityButton" />
											</Panel>
										<!-- AUTOCAST -->											
											<Panel id="AbilityBoxesAutoCast" class="LeftRightFlow">
												<Panel class="AutocastContainer">
													<DOTASettingsKeyBinder bind="AbilityPrimary1AutoCast" class="HeroAbilityBindAbilityButton" />
												</Panel>
												<Panel class="AutocastContainer">
													<DOTASettingsKeyBinder bind="AbilityPrimary2AutoCast" class="HeroAbilityBindAbilityButton" />
												</Panel>
												<Panel class="AutocastContainer">
													<DOTASettingsKeyBinder bind="AbilityPrimary3AutoCast" class="HeroAbilityBindAbilityButton" />
												</Panel>
												<Panel class="AutocastContainer">
													<DOTASettingsKeyBinder bind="AbilitySecondary1AutoCast" class="HeroAbilityBindAbilityButton" />
												</Panel>
												<Panel class="AutocastContainer">
													<DOTASettingsKeyBinder bind="AbilitySecondary2AutoCast" class="HeroAbilityBindAbilityButton" />
												</Panel>
												<Panel class="AutocastContainer">
													<DOTASettingsKeyBinder bind="AbilityUltimateAutoCast" class="HeroAbilityBindAbilityButton" />
												</Panel>
											</Panel>
										<!-- QUICKCAST -->											
											<Panel id="AbilityBoxesQuickcast" class="LeftRightFlow">
												<DOTASettingsKeyBinder bind="AbilityPrimary1QuickCast" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder bind="AbilityPrimary2QuickCast" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder bind="AbilityPrimary3QuickCast" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder bind="AbilitySecondary1QuickCast" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder bind="AbilitySecondary2QuickCast" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder bind="AbilityUltimateQuickCast" class="HeroAbilityBindAbilityButton" />
											</Panel>
											<Panel id="HeroAbilityBindTypeButtons">
												<RadioButton group="HeroCastType" id="HeroCastButton" class="HeroAbilityBindTypeButton" text="#dota_settings_cast" onactivate="DOTASetActiveAbilityHeroSubTab(0);" onmouseover="DOTAShowTextTooltip( #DOTA_Hotkeys_Tooltip_Cast)" onmouseout="DOTAHideTextTooltip()"	/>
												<RadioButton group="HeroCastType" id="HeroAutoCastButton" class="HeroAbilityBindTypeButton" text="#dota_settings_autocast" onactivate="DOTASetActiveAbilityHeroSubTab(1);" onmouseover="DOTAShowTextTooltip( #DOTA_Hotkeys_Tooltip_Autocast)" onmouseout="DOTAHideTextTooltip()" />
												<RadioButton group="HeroCastType" id="HeroQuickCastButton" class="HeroAbilityBindTypeButton" text="#dota_settings_quickcast" onactivate="DOTASetActiveAbilityHeroSubTab(2);"	onmouseover="DOTAShowTextTooltip( #DOTA_Hotkeys_Tooltip_Quickcast)" onmouseout="DOTAHideTextTooltip()" />
											</Panel>
										</Panel>

										<!-- AUTOCAST -->
										<Panel id="AbilityBindings_Neutrals" class="TopBottomFlow">
											<Panel id="AbilityBoxes">
												<DOTASettingsKeyBinder text="#dota_settings_abilities_neutral1" bind="NeutralAbility1" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder text="#dota_settings_abilities_neutral2" bind="NeutralAbility2" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder text="#dota_settings_abilities_neutral3" bind="NeutralAbility3" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder text="#dota_settings_abilities_neutral4" bind="NeutralAbility4" class="HeroAbilityBindAbilityButton" />
											</Panel>
											<Panel class="AbilityBindingOptions">
												<ToggleButton id="NeutralMirrorButton" class="CheckBox MirrorHotkeysCheckBox" text="#dota_settings_mirror"	onactivate="DOTASetMirrorKeys();"/>
											</Panel>
										</Panel>

										<!-- QUICKCAST -->
										<Panel id="AbilityBindings_Courier" class="TopBottomFlow">
											<Panel id="AbilityBoxes">
												<DOTASettingsKeyBinder text="#dota_settings_abilities_courier1" bind="CourierAbility1" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder text="#dota_settings_abilities_courier2" bind="CourierAbility2" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder text="#dota_settings_abilities_courier3" bind="CourierAbility3" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder text="#dota_settings_abilities_courier4" bind="CourierAbility4" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder text="#dota_settings_abilities_courier5" bind="CourierAbility5" class="HeroAbilityBindAbilityButton" />
												<DOTASettingsKeyBinder text="#dota_settings_abilities_courier6" bind="CourierAbility6" class="HeroAbilityBindAbilityButton" />
											</Panel>
											<Panel class="AbilityBindingOptions">
												<ToggleButton id="CourierMirrorButton" class="CheckBox MirrorHotkeysCheckBox" text="#dota_settings_mirror"	onactivate="DOTASetMirrorKeys();"/>
											</Panel>
										</Panel>
									</Panel>
								</Panel>

								<Panel id="AbilitiesDisabled">
									<Label text="#DOTA_Settings_LegacyKeysActive" />
								</Panel>
							</Panel>
							<DOTASettingsKeyBinder text="#dota_settings_abilitylearn" bind="LearnAbilities" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_LearnAbility, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_upgradestats" bind="LearnStats" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_LearnStats, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
						</Panel>

						<Panel id="UnitActions" class="SettingsSection">
							<Label text="#dota_settings_unitactions" class="SectionHeader"/>
							<Panel class="SectionHeaderLineNoMargin"/>
							<DOTASettingsKeyBinder text="#dota_settings_attack" bind="HeroAttack" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_HeroAttack, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_move" bind="HeroMove" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_HeroMove, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_patrol" bind="HeroPatrol" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_HeroPatrol, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_stop" bind="HeroStop" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_HeroStop, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_holdposition" bind="HeroHold" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_HeroHold, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_selecthero" bind="HeroSelect" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_HeroSelect, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_selectall" bind="SelectAll" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_SelectAll, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_selectallothers" bind="SelectAllOthers" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_SelectAllOthers, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_selectcourier" bind="CourierSelect" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_CourierSelect, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_courierdeliver" bind="CourierDeliver" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_CourierDeliver, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_courierhaste" bind="CourierHaste" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_CourierHaste, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_useactionitem" bind="UseActionItem" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_UseActionItem, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_herotaunt" bind="HeroTaunt" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_HeroTaunt, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
						</Panel>
					</Panel>

					<Panel id="Column1" class="HotKeySettingsColumn">
						<Panel id="Items" class="SettingsSection">
							<Label text="#dota_settings_items" class="SectionHeader" />
							<Panel class="SectionHeaderLineNoMargin"/>

							<Panel id="ItemBindingContents">

								<Panel id="ItemCast" class="ItemList">
									<Panel class="ItemBindButtonContainer">
										<DOTASettingsKeyBinder class="ItemBindButton" bind="Inventory1" />
										<DOTASettingsKeyBinder class="ItemBindButton" bind="Inventory2" />
										<DOTASettingsKeyBinder class="ItemBindButton" bind="Inventory3" />
									</Panel>
									<Panel class="ItemBindButtonContainer">
										<DOTASettingsKeyBinder class="ItemBindButton" bind="Inventory4" />
										<DOTASettingsKeyBinder class="ItemBindButton" bind="Inventory5" />
										<DOTASettingsKeyBinder class="ItemBindButton" bind="Inventory6" />
									</Panel>
								</Panel>
								<Panel id="ItemQuickcast" class="ItemList">
									<Panel class="ItemBindButtonContainer">
										<DOTASettingsKeyBinder class="ItemBindButton" bind="Inventory1Quickcast" />
										<DOTASettingsKeyBinder class="ItemBindButton" bind="Inventory2Quickcast" />
										<DOTASettingsKeyBinder class="ItemBindButton" bind="Inventory3Quickcast" />
									</Panel>
									<Panel class="ItemBindButtonContainer">
										<DOTASettingsKeyBinder class="ItemBindButton" bind="Inventory4Quickcast" />
										<DOTASettingsKeyBinder class="ItemBindButton" bind="Inventory5Quickcast" />
										<DOTASettingsKeyBinder class="ItemBindButton" bind="Inventory6Quickcast" />
									</Panel>
								</Panel>
								<Panel id="ItemBindTypeButtons">
									<RadioButton class="ItemBindTypeButton" id="ItemCastButton" group="SettingsItemBinds" text="#dota_settings_cast" onactivate="DOTASetActiveItemSubTab(0);" onmouseover="DOTAShowTextTooltip( #DOTA_Hotkeys_Tooltip_ItemCast)" onmouseout="DOTAHideTextTooltip()" />
									<RadioButton class="ItemBindTypeButton" id="ItemQuickCastButton" group="SettingsItemBinds" text="#dota_settings_quickcast" onactivate="DOTASetActiveItemSubTab(1);" onmouseover="DOTAShowTextTooltip( #DOTA_Hotkeys_Tooltip_ItemQuickcast)" onmouseout="DOTAHideTextTooltip()" />
								</Panel>
							</Panel>
							<DOTASettingsKeyBinder text="#dota_settings_openshop" bind="ShopToggle" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_ShopToggle, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_purchasequickbuy" bind="PurchaseQuickbuy" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_PurchaseQuickbuy, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_purchasesticky" bind="PurchaseSticky" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_PurchaseSticky, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_grabstashitems" bind="GrabStashItems" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_GrabStashItems, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
						</Panel>
						<Panel id="CameraActions" class="SettingsSection">
							<Label text="#dota_settings_cameraactions" class="SectionHeader" />
							<Panel class="SectionHeaderLineNoMargin"/>
							<DOTASettingsKeyBinder text="#dota_settings_cameraup" bind="CameraUp" />
							<DOTASettingsKeyBinder text="#dota_settings_cameradown" bind="CameraDown" />
							<DOTASettingsKeyBinder text="#dota_settings_cameraleft" bind="CameraLeft" />
							<DOTASettingsKeyBinder text="#dota_settings_cameraright" bind="CameraRight"/>
							<DOTASettingsKeyBinder text="#dota_settings_cameragrip" bind="CameraGrip" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_CameraGrip, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_recentevent" bind="RecentEvent" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_RecentEvent, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_inspectheroinworld" bind="InspectHeroInWorld" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_InspectHeroInWorld, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
						</Panel>
					</Panel>

					<Panel id="Column2" class="HotKeySettingsColumn">
						<Panel id="Chat" class="SettingsSection">
							<Label text="#dota_settings_chat" class="SectionHeader" />
							<Panel class="SectionHeaderLineNoMargin"/>

							<Panel id="ChatBindDialogsContents">

								<Button id="PhrasesButton" class="ChatDialogButton" onactivate="DOTAShowPhrasesPage();" >
									<Image src="s2r://panorama/images/settings_phrases_png.vtex" class="ChatDialogButtonImage" />
									<Label text="#dota_settings_phrases"	class="ChatBindDialogButtonLabel" />
								</Button>
								<Button id="ChatWheelButton" class="ChatDialogButton" onactivate="DOTAShowChatWheelPage();" >
									<Image src="s2r://panorama/images/settings_chatwheel_png.vtex" class="ChatDialogButtonImage" />
									<Label text="#dota_settings_chatwheel_page" class="ChatBindDialogButtonLabel" />
								</Button>

							</Panel>

							<DOTASettingsKeyBinder id="ChatTeam" text="#dota_settings_chatteam" bind="ChatTeam" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_ChatTeam, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_chatglobal" bind="ChatGlobal" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_ChatGlobal, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_chatwheel_show" bind="ChatWheel" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_ChatWheel, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							
							<DOTASettingsKeyBinder text="#dota_settings_chatvoice_party" bind="ChatVoiceParty" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_ChatVoiceParty, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />

							<DOTASettingsKeyBinder text="#dota_settings_chatvoice_team" bind="ChatVoiceTeam" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_ChatVoiceTeam, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
				
							
							
						</Panel>

						<Panel id="PlayerActions" class="SettingsSection">
							<Label text="#dota_settings_playeractions" class="SectionHeader" />
							<Panel class="SectionHeaderLineNoMargin"/>
							<DOTASettingsKeyBinder text="#dota_settings_activateglyph" bind="ActivateGlyph" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_ActivateGlyph, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_scoreboardtoggle" bind="ScoreboardToggle" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_ScoreboardToggle, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_pause" bind="Pause" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_Pause, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_screenshot" bind="ScreenshotSettings" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_ScreenshotSettings, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
							<DOTASettingsKeyBinder text="#dota_settings_console" bind="Console" onmouseover="DOTAShowTextTooltipStyled( #DOTA_Hotkeys_Tooltip_Console, BindingsTooltip)" onmouseout="DOTAHideTextTooltip()" />
						</Panel>

						<Panel id="ControlGroups" class="SettingsSection">
							<Label text="#dota_settings_controlgroups" class="SectionHeader" />
