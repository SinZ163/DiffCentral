						<DOTASettingsCheckbox id="VoiceAlwaysSampleMic" convar="voice_always_sample_mic" text="#dota_settings_voice_always_sample_mic" onmouseover="DOTAShowTitleTextTooltip( #dota_settings_voice_always_sample_mic, #dota_settings_voice_always_sample_mic_tooltip )" onmouseout="DOTAHideTitleTextTooltip()" />

