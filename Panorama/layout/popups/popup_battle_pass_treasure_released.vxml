<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_splash_ad.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_battle_pass_treasure_released.vcss_c" />
	</styles>


	<PopupSplashAd class="PopupPanel Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked()">
		<Panel id="MainPanel">
			<Panel id="TopHalfImage" >
				<Panel id="Background_Treasure4" class="BackgroundPromoArt"/>
				<Button class="CloseButton" onactivate="UIPopupButtonClicked()"/>
			</Panel>
			<Panel id="Footer">
				<Label id="Title" class="PopupTitle" text="#DOTA_SplashAd_WinterMajorBattlePass_AddedTreasure4" html="true"/>
				<Label id="Message" text="#DOTA_SplashAd_WinterMajorBattlePass_AddedTreasure4Desc_Owns" html="true"/>
				<Label id="MessageNotOwned" text="#DOTA_SplashAd_WinterMajorBattlePass_AddedTreasure4Desc" html="true"/>
				<Panel id="Buttons" class="PopupButtonRow" >
					<Button id="ViewDetailsButton" class="ButtonBevel" onactivate="DOTAFrontPageBattlePassClick() UIPopupButtonClicked()">
						<Label id="ButtonView" text="#DOTA_SplashAd_WinterMajorBattlePass_View"/>
						<Label id="ButtonPreview" text="#DOTA_SplashAd_WinterMajorBattlePass_Preview"/>
					</Button>
				</Panel>
			</Panel>
		</Panel>
		<Panel id="AddedPanel">
			<Panel id="CompendiumImage"/>
			<Panel id="AddedPanelContent" class="TopBottomFlow">
				<Label id="ContentTitle" text="#DOTA_SplashAd_WinterMajorBattlePass_AddedTitle"/>
				<Panel id="List" class="TopBottomFlow">
					<Label id="No1" text="#DOTA_SplashAd_WinterMajorBattlePass_List1" html="true"/>
					<Label id="No2" text="#DOTA_SplashAd_WinterMajorBattlePass_List2" html="true"/>
					<Label id="No3" text="#DOTA_SplashAd_WinterMajorBattlePass_List3" html="true"/>
				</Panel>
			</Panel>
		</Panel>		
	</PopupSplashAd>
</root>
