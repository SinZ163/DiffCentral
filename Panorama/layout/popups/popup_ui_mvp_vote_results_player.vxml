<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_ui_mvp_vote.vcss_c" />
	</styles>

  <Panel class="ResultsPlayerRow">
    <Label class="Place" text="{s:mvp_place}" />
		<Panel class="RadiantTeamResultsDefault">
			<Panel class="RadiantTeamLogoDefault ResultsLobbyTeamIconDefault" />
			<Panel id="ShortRadiantColorBarDefault" />
		</Panel>
		<Panel class="RadiantTeamResultsCustom">
			<DOTAUGCImage class="RadiantTeamLogo ResultsLobbyTeamIcon" />
			<Panel id="ShortRadiantColorBarCustom" />
		</Panel>
		<Panel class="DireTeamResultsDefault">
			<Panel class="DireTeamLogoDefault ResultsLobbyTeamIconDefault" />
			<Panel id="ShortDireColorBarDefault" />
		</Panel>
		<Panel class="DireTeamResultsCustom">
			<DOTAUGCImage class="DireTeamLogo ResultsLobbyTeamIcon" />
			<Panel id="ShortDireColorBarCustom" />
		</Panel>
	
		<DOTAHeroImage id="HeroIcon" heroimagestyle="landscape" scaling="stretch-to-fit-preserve-aspect" />
		<DOTAHeroMovie id="HeroMovie" />
	
	
    <Label class="PlayerName" text="{s:player_name}" />
    <Label id="PlayerVotePercent" text="{s:player_vote_percent}" />     
  </Panel>
	
</root>