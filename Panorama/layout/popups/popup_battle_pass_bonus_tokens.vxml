<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_splash_ad.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_battle_pass_treasure_released.vcss_c" />
	</styles>


	<PopupSplashAd class="PopupPanel Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked()">
		<Panel id="TokensGivenMainPanel" class="TopBottomFlow">
				<Label id="Title" class="PopupTitle" text="#DOTA_Compendium_Wagering_TokensPopup_Title" html="true"/>
				<Label id="TokenMessage" text="#DOTA_Compendium_Wagering_TokensPopup" html="true"/>
				<Panel id="Buttons" class="PopupButtonRow" >
					<Button id="ContinueButton" class="ButtonBevel" onactivate="DOTAFrontPageBattlePassClick() UIPopupButtonClicked()">
						<Label id="ButtonView" text="#DOTA_Continue"/>
					</Button>
				</Panel>

		</Panel>
	</PopupSplashAd>
</root>
