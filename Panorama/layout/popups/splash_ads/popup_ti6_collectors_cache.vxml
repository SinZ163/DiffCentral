<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/popups/splash_ads/popup_ti6_collectors_cache.vcss_c" />
	</styles>

	<Popup class="PopupPanel PopupTI6CollectorsCache Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked()">

		<Panel class="Contents">
			

			<Panel id="SplashImage">
				
				
				<Panel class="SplashImageShading" />
				
				<Panel class="SplashImageOverlay">
					<Panel class="OverlayText">
						<Label class="OverlayHeader" text="#DOTA_SplashAd_TI6_CollectorsCache_Exclusive_Header" />
						<Label class="OverlayDetails" text="#DOTA_SplashAd_TI6_CollectorsCache_Exclusive_Text" />
					</Panel>
					<Panel class="OverlayText">
						<Label class="OverlayHeader" text="#DOTA_SplashAd_TI6_CollectorsCache_Community_Header" />
						<Label class="OverlayDetails" text="#DOTA_SplashAd_TI6_CollectorsCache_Community_Text" />
					</Panel>
					<Panel class="OverlayText">
						<Label class="OverlayHeader" text="#DOTA_SplashAd_TI6_CollectorsCache_Contribute_Header" />
						<Label class="OverlayDetails" text="#DOTA_SplashAd_TI6_CollectorsCache_Contribute_Text" />
					</Panel>
				</Panel>

				<TextButton id="DetailsButton" class="ButtonBevel" text="#DOTA_View_Details" onactivate="UIPopupButtonClicked( DOTAShowItemDefPage( 11573 ) )" />
				
				<Label id="Title" text="#DOTA_SplashAd_TI6_CollectorsCache_Title" />
			</Panel>
		</Panel>

		<Button class="CloseButton"  onactivate="UIPopupButtonClicked()" />

	</Popup>
</root>
