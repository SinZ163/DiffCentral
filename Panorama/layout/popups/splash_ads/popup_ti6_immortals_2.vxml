<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/popups/splash_ads/popup_ti6_immortals_2.vcss_c" />
	</styles>

	<Popup class="PopupPanel PopupTI6Immortals2 Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked()">

		<Panel class="Contents">

            <Panel id="TitleBox">
                <Label id="Title" text="#DOTA_SplashAd_TI6_Immortals_2_Title" />
                <Label id="SubTitle" text="#DOTA_SplashAd_TI6_Immortals_2_Available" />
            </Panel>

            <Panel id="SplashImage">
				

			</Panel>

            <Panel class="SplashImageOverlay">
                <Panel class="OverlayText">
                    <Panel id="CenterText">
                        <Panel id="EscalatingIcon" />
                        <Label class="OverlayHeader" text="#DOTA_SplashAd_TI6_Immortals_2_Escalating" />
                    </Panel>
                    <Label class="OverlayDetails" text="#DOTA_SplashAd_TI6_Immortals_2_Escalating_Desc" />
                </Panel>
                <TextButton id="DetailsButton" class="ButtonBevel" text="#DOTA_View_Details" onactivate="UIPopupButtonClicked( DOTAShowItemDefPage( 11520 ) )" />
            </Panel>
        </Panel>

		<Button class="CloseButton"  onactivate="UIPopupButtonClicked()" />

	</Popup>
</root>
