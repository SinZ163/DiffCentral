<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_item_picker.vcss_c" />
	</styles>

	<PopupItemPicker class="PopupPanel Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked( DOTAItemPickerCancelled() )">
		<Label id="Title" class="PopupTitle" />

		<EconItemImage id="HeaderItemImage" />

		<Label id="Description" class="Description" />

		<Panel id="Choices">
			<!-- dynamic -->
		</Panel>
			
		<Panel class="PopupButtonRow">
			<Button class="PopupButton" onactivate="UIPopupButtonClicked( DOTAItemPickerCancelled() )">
				<Label text="#DOTA_Cancel" />
			</Button>
		</Panel>
	</PopupItemPicker>
</root>
