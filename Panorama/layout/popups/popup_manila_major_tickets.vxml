<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_splash_ad.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_battle_pass_treasure_released.vcss_c" />
	</styles>


	<PopupSplashAd class="PopupPanel Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked()">
		
		<Panel id="ManilaMainPanel" class="TopBottomFlow">
				<Label id="Title" class="PopupTitle" text="#DOTA_Manila_Major_Popup_Title" html="true"/>
				<Label id="ManilaMessage" text="#DOTA_Manila_Major_Popup" html="true"/>
				<Panel id="Buttons" class="PopupButtonRow" >
					<Button id="BuyTicketsButton" class="ButtonBevel" onactivate="BrowserGoToURL( https://smtickets.com/events/view/4285 ) UIPopupButtonClicked()">
						<Label id="ButtonView" text="#DOTA_Buy_Tickets_Popup"/>
					</Button>			
				</Panel>
				
		</Panel>
		<Button class="CloseButton" id="ManilaCloseButton" onactivate="UIPopupButtonClicked()" />
	</PopupSplashAd>
</root>
