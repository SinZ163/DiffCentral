<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_custom_game_find_private.vcss_c" />
	</styles>

	<PopupCustomGameFindPrivate class="Hidden" defaultfocus="PasswordTextEntry" popupbackground="dim" >

	<Label text="#DOTA_Custom_Lobby_Find_Private_Title" class="Title" />

	<Panel class="TextEntryContainer">
		<Label text="#DOTA_Custom_Lobby_Find_Private_Desc" class="EnterPasswordDesc" />
		<TextEntry id="PasswordTextEntry" />
	</Panel>

	<Panel class="ButtonFooter">
		<Button id="FindPrivateConfirmButton" onactivate="DOTAFindPrivateCustomLobbyConfirm()">
			<Label text="#DOTA_Ok"/>
		</Button>      
		<Button id="FindPrivateCancelButton" onactivate="DOTAFindPrivateCustomLobbyCancel()">
			<Label text="#UI_Cancel" />
		</Button>		
	</Panel>
	
	</PopupCustomGameFindPrivate>
</root>
