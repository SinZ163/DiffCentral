<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_profile_level_up.vcss_c" />
	</styles>

	<PopupProfileLevelUp class="PopupPanel Hidden" popupbackground="blur" hittest="false" oncancel="UIPopupButtonClicked()" >
		<Panel class="BlackBackground"/>
    	<DOTAScenePanel id="TrophyLvlUp" map="scenes/badge_level_up" camera="shot_camera"/>
		<Panel class="MainContainer">
			<Label id="ProfileBadgeUpgradedLabel" text="#DOTA_ProfileBadge_Upgraded" />
			<DOTAProfileBadge id="ProfileBadge" />
			<Button id="OkButton" class="PopupButton" onactivate="UIPopupButtonClicked()">
				<Label text="#DOTA_Ok" />
			</Button>
		</Panel>
	</PopupProfileLevelUp>
</root>
