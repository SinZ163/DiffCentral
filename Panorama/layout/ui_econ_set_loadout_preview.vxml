<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/ui_econ_set_loadout_preview.vcss_c" />
	</styles>

	<DOTAUIEconSetLoadoutPreview class="EconItemSetFrame" >
		<Panel class="EconItemSetDisplay">
			<DOTAUIEconSetPreview id="EconSetPreview" />
			<Panel class="LeftRightFlow SetIcons" >
        <Panel class="InformationalIcon SeasonalIcon" onmouseover="UIShowTextTooltip( #DOTA_EconItemSet_Tooltip_BattlePass )" onmouseout="UIHideTextTooltip(); DOTAUIEconItemRetriggerOnMouseOver()" />
        <Panel class="InformationalIcon Purgatory" onmouseover="UIShowTextTooltip( #DOTA_EconItemSet_Tooltip_Purgatory )" onmouseout="UIHideTextTooltip(); DOTAUIEconItemRetriggerOnMouseOver()"  />
        <Panel class="InformationalIcon EconItemSetBundle" onmouseover="UIShowTextTooltip( #DOTA_LoadoutSet_Tooltip_InBundle )" onmouseout="UIHideTextTooltip()"  />
				<Panel class="InformationalIcon EconItemSetPartialSet" onmouseover="UIShowTextTooltip( #DOTA_LoadoutSet_Tooltip_PartialSet )" onmouseout="UIHideTextTooltip()" />
				<Panel class="InformationalIcon EconItemSetMultiStyle" onmouseover="UIShowTextTooltip( #DOTA_EconItemSet_Tooltip_MultipleStyles )" onmouseout="UIHideTextTooltip()" />
        <Panel class="InformationalIcon EconItemSetMultiTeam" onmouseover="UIShowTextTooltip( #DOTA_EconItemSet_Tooltip_MultipleTeams )" onmouseout="UIHideTextTooltip()" />
        <Panel class="InformationalIcon EmptySocketIcon" onmouseover="UIShowTextTooltip( #DOTA_EconItemSet_Tooltip_HasEmptySocket )" onmouseout="UIHideTextTooltip()" />
        <Panel class="InformationalIcon GemIcon" onmouseover="UIShowTextTooltip( #DOTA_EconItemSet_Tooltip_HasCommonGem )" onmouseout="UIHideTextTooltip()" />
        <Panel class="InformationalIcon RareGemIcon" onmouseover="UIShowTextTooltip( #DOTA_EconItemSet_Tooltip_HasRareGem )" onmouseout="UIHideTextTooltip()" />
      </Panel>
			<Panel class="EconItemSetInfo">
				<Label class="EconItemSetName" text="#DOTA_EconItemSet_Name" />
				<Label class="EconItemSetRarityName ItemRarityColor" text="#DOTA_EconItemSet_Rarity" />
			</Panel>
			<Label class="DeveloperOnlyLabel" text="#DOTA_EconItemSet_DeveloperOnly" />
      <Label class="TestItemLabel" text="#DOTA_EconItemSet_TestItem" />
      <Panel id ="EconItemSetEquipped" hittest="false"/>
		</Panel>
		<Panel class="EconItemSetRarity ItemRarityBackgroundColor" />
		<Panel class="EconItemSetPriceFrame" >
			<Panel class="LeftRightFlow">
				<Label class="EconItemSetStatus" text="#DOTA_EconItemSet_Status" />
				<Label class="EconItemSetSaleAmount" text="#DOTA_EconItemSet_SaleAmount" />
			</Panel>
			<Panel class="PriceContainer">
				<Panel class="TreasureIcon" />
				<Label class="EconItemSetPrice" text="#DOTA_EconItemSet_Price" />
			</Panel>
		</Panel>
	</DOTAUIEconSetLoadoutPreview>
	
</root>
