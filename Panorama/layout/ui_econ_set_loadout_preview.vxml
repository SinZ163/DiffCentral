<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/ui_econ_set_loadout_preview.vcss_c" />
	</styles>

	<DOTAUIEconSetLoadoutPreview class="EconItemSetFrame" >
		<Panel class="EconItemSetDisplay">
			<DOTAUIEconSetPreview id="EconSetPreview" />
			<Panel class="LeftRightFlow SetIcons" >
        <Panel class="Purgatory" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_Purgatory )" onmouseout="DOTAHideTextTooltip(); DOTAUIEconItemRetriggerOnMouseOver()"  />
        <Panel class="EconItemSetBundle" onmouseover="DOTAShowTextTooltip( #DOTA_LoadoutSet_Tooltip_InBundle )" onmouseout="DOTAHideTextTooltip()"  />
				<Panel class="EconItemSetPartialSet" onmouseover="DOTAShowTextTooltip( #DOTA_LoadoutSet_Tooltip_PartialSet )" onmouseout="DOTAHideTextTooltip()" />
				<Panel class="EconItemSetMultiStyle" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_MultipleStyles )" onmouseout="DOTAHideTextTooltip()" />
        <Panel class="EconItemSetMultiTeam" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_MultipleTeams )" onmouseout="DOTAHideTextTooltip()" />
        <Panel class="EmptySocketIcon" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_HasEmptySocket )" onmouseout="DOTAHideTextTooltip()" />
        <Panel class="GemIcon" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_HasCommonGem )" onmouseout="DOTAHideTextTooltip()" />
        <Panel class="RareGemIcon" onmouseover="DOTAShowTextTooltip( #DOTA_EconItemSet_Tooltip_HasRareGem )" onmouseout="DOTAHideTextTooltip()" />
      </Panel>
			<Panel class="EconItemSetInfo">
				<Label class="EconItemSetName" text="#DOTA_EconItemSet_Name" />
				<Label class="EconItemSetRarityName ItemRarityColor" text="#DOTA_EconItemSet_Rarity" />
			</Panel>
			<Label class="DeveloperOnlyLabel" text="#DOTA_EconItemSet_DeveloperOnly" />
			<Panel id ="EconItemSetEquipped" hittest="false"/>
		</Panel>
		<Panel class="EconItemSetRarity ItemRarityBackgroundColor" />
		<Panel class="EconItemSetPriceFrame" >
			<Panel class="LeftRightFlow">
				<Label class="EconItemSetStatus" text="#DOTA_EconItemSet_Status" />
				<Label class="EconItemSetSaleAmount" text="#DOTA_EconItemSet_SaleAmount" />
			</Panel>
			<Panel class="PriceContainer">
				<Panel class="TreasureIcon" />
				<Label class="EconItemSetPrice" text="#DOTA_EconItemSet_Price" />
			</Panel>
		</Panel>
	</DOTAUIEconSetLoadoutPreview>
	
</root>
