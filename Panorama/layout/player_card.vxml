<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/player_card.vcss_c" />
	</styles>

	<DOTAPlayerCard>
		<Image id="PlayerImage"/>
		<Label class="PlayerName" text="{g:dota_persona:pro_short:b:account_id}" html="true"/>
		<Label class="TeamName" text="{s:team_name}"/>
		<Panel id="BonusContainer">
			<Label id="Bonus0" class="Bonus" text="#DOTA_PlayerCardBonus" html="true"/>
			<Label id="Bonus1" class="Bonus" text="#DOTA_PlayerCardBonus" html="true"/>
			<Label id="Bonus2" class="Bonus" text="#DOTA_PlayerCardBonus" html="true"/>
			<Label id="Bonus3" class="Bonus" text="#DOTA_PlayerCardBonus" html="true"/>
			<Label id="Bonus4" class="Bonus" text="#DOTA_PlayerCardBonus" html="true"/>
		</Panel>
		<Label class="SilverCount" text="#DOTA_PlayerCardSilverCount"/>
		<Label class="GoldCount" text="#DOTA_PlayerCardGoldCount"/>
	</DOTAPlayerCard>

</root>
