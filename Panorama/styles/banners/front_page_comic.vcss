
#FrontpageCellComicSunderedMoon
{
//	overflow: noclip;
}

#FrontpageCellComicSunderedMoon:hover #SunderedMoon
{
	transform: rotateY(-165deg) rotateX(25deg) rotateZ(6deg);	
}

#FrontpageCellComicSunderedMoon:hover .ComicPage
{
	pre-transform-scale2d: 1;
}

#FrontpageCellComicSunderedMoon:hover #InsidePanel
{
	wash-color: white;
}

#FrontpageCellComicSunderedMoon:hover #CoverImage
{
	opacity: 0;
	wash-color: black;
}

#FrontpageCellComicSunderedMoon:hover #CoverBackground
{
	wash-color: white;
}


#FrontpageCellComicSunderedMoon:hover #ImagePanel
{
	transform: translateX( 0px ) translateY( -10px ) rotateZ(0deg);
}

#ImagePanel
{
	padding-top: 16px;
	transform: translateX( -60px ) translateY( -20px ) rotateZ(-10deg);
	width: 400px;
	height: 400px;
	//border: 1px solid red;

	transition-property: transform;
	transition-timing-function: ease-in-out;
	transition-duration: 0.5s;	
}


#BackgroundStrip
{
	width: 100%;
	height: 100%;
	background-image: url("s2r://panorama/images/banners/comic/comicbg_sunderedmoon_psd.vtex");
	background-size: contain;
	pre-transform-scale2d: 1.1;

	animation-name: BackgroundMotion;
	animation-delay: 0s;
	animation-duration: 17s;
	animation-timing-function: ease-in-out;
	animation-iteration-count: infinite;	
}

@keyframes 'BackgroundMotion'
{
	0%		{	transform: translateX( -10px ) rotateZ( 0deg ); }
	50%		{	transform: translateX( 10px ) rotateZ( 3deg ); }
	100%	{	transform: translateX( -10px ) rotateZ( 0deg ); }
}

.ComicPage
{
	width: 290px;
	height: 200px;
	horizontal-align: center;
	transform: rotateY(0deg) rotateX(26deg) rotateZ(6deg);
//	transform-origin: 0% 0%;
	border-radius: 0.1px;
	pre-transform-scale2d: 0.9;

	transition-property: transform, pre-transform-scale2d, wash-color;
	transition-timing-function: ease-in-out;
	transition-duration: 0.5s;
}

#SunderedMoon
{
	transition-duration: 0.5s, 0.5s, 0.25s ;
}



#InsidePanel
{

	wash-color: #000000dd;
	transform: rotateY(2deg) rotateX(26deg) rotateZ(6deg);
}

.Page
{
	width: 145px;
	height: 200px;
	background-image: url("s2r://panorama/images/banners/comic/cover_sunderedmoon_png.vtex");
	background-color: gray;	
	background-size: cover;	
	horizontal-align: right;
}

#InsidePage
{
	background-image: url("s2r://panorama/images/banners/comic/cover_sunderedmoon_inside_png.vtex");
}

#CoverImage
{
	width: 145px;
	height: 200px;
	background-image: url("s2r://panorama/images/banners/comic/cover_sunderedmoon_png.vtex");
	wash-color: white;
	opacity: 1;
	background-size: cover;	

	transition-property: opacity, wash-color;
	transition-timing-function: ease-in-out;
	transition-duration: 0.5s;
}

#CoverBackground
{
	wash-color: black;
	background-image: url("s2r://panorama/images/banners/comic/cover_sunderedmoon_inside_left_png.vtex");
	transform: scaleX(-1);
	background-size: contain;
	background-color: #444444;
	background-color: gradient( linear, 100% 0%, 0% 0%, from( #444444 ), to( #333333 ) );
	width: 145px;
	height: 200px;
	border: 1px solid black;
	transition-property: wash-color;
	transition-timing-function: ease-in-out;
	transition-duration: 0.1s;	
	transition-delay: 0.23s;	
}

.Empty
{
	width: 145px;
	height: 200px;
	background-color: transparent;
	horizontal-align: left;
}


#CustomHeader
{
	background-color: black;
	visibility: visible;
	color: #528EA5;
	text-shadow: none;

	transition-property: color;
	transition-duration: 0.2s;
	transition-timing-function: ease-in-out;
}

#CustomFooterText
{
	visibility: visible;
	color: #cccccc;
	transition-property: color;
	transition-duration: 0.2s;
	transition-timing-function: ease-in-out;
	text-shadow: 0px 1px 3px 3.0 #000000;
}

#FrontpageCellComicSunderedMoon:hover #CustomHeader
{
	color: white;
}

#CustomFooter
{
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #00000000 ), color-stop( 0.7, #0000004f  ), color-stop( 0.75, #0000006f  ), to( #000000ff ) );	
	vertical-align: bottom;
	width: 100%;
	height: 70%;
}

#CustomFooterDetails
{
	visibility: visible;
	vertical-align: bottom;
	margin-bottom: 32px;
	z-index: 2;
	color: #808f8d;
}

#FrontpageCellComicSunderedMoon:hover #CustomFooterDetails
{
	opacity: 1;
}


#FrontpageCellComicSunderedMoon:hover .DOTATodayCellFooter
{
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #00000000 ), color-stop( 0.7, #0000003f  ), color-stop( 0.75, #0000003f  ), to( #000000ff ) );
}

#FrontpageCellComicSunderedMoon .DOTATodayCellFooter
{
	background-color:  gradient( linear, 0% 0%, 0% 100%, from( #00000000 ), color-stop( 0.7, #00000000  ), color-stop( 0.75, #00000000  ), to( #00000000 ) );
}