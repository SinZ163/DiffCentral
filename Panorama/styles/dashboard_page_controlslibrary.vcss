.LSVElement
{
	width: 64px;
	height: width-percentage( 73% );

	transition-property: position!immediate;
	transition-delay: 0.0s;
	transition-duration: 0.2s;
	transition-timing-function: ease-in-out;
}


.LSVExample .ListSegmentDisplayed1,
.LSVExample .ListSegmentHiddenBefore1,
.LSVExample .ListSegmentHiddenAfter1
{
	x: 0px;
	y: 0px;
}

.LSVExample .ListSegmentDisplayed2,
.LSVExample .ListSegmentHiddenBefore2,
.LSVExample .ListSegmentHiddenAfter2
{
	x: 64px;
	y: 0px;
}

.LSVExample .ListSegmentDisplayed3,
.LSVExample .ListSegmentHiddenBefore3,
.LSVExample .ListSegmentHiddenAfter3
{
	x: 128px;
	y: 0px;
}

.LSVExample .ListSegmentDisplayed4,
.LSVExample .ListSegmentHiddenBefore4,
.LSVExample .ListSegmentHiddenAfter4
{
	x: 192px;
	y: 0px;
}

.LSVExample .ListSegmentHiddenBefore1,
.LSVExample .ListSegmentHiddenBefore2,
.LSVExample .ListSegmentHiddenBefore3,
.LSVExample .ListSegmentHiddenBefore4
{
	y: -48px;
}

.LSVExample .ListSegmentHiddenAfter1,
.LSVExample .ListSegmentHiddenAfter2,
.LSVExample .ListSegmentHiddenAfter3,
.LSVExample .ListSegmentHiddenAfter4
{
	y: 48px;
}

.LSVExample .ListSegmentHiddenBefore1,
.LSVExample .ListSegmentDisplayed1,
.LSVExample .ListSegmentHiddenAfter1
{
	transition-delay: 0.0s;
}

.LSVExample .ListSegmentHiddenBefore2,
.LSVExample .ListSegmentDisplayed2,
.LSVExample .ListSegmentHiddenAfter2
{
	transition-delay: 0.05s;
}

.LSVExample .ListSegmentHiddenBefore3,
.LSVExample .ListSegmentDisplayed3,
.LSVExample .ListSegmentHiddenAfter3
{
	transition-delay: 0.1s;
}

.LSVExample .ListSegmentHiddenBefore4,
.LSVExample .ListSegmentDisplayed4,
.LSVExample .ListSegmentHiddenAfter4
{
	transition-delay: 0.15s;
}

