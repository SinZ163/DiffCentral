@keyframes 'blur-pulse'
{
	0%
	{
		blur: gaussian( 0, 0, 0 );
	}
	
	25%
	{
		blur: gaussian( 50, 0.1, 6 );
	}

	50%
	{
		blur: gaussian( 0, 0, 0 );
	}
	
	75%
	{
		blur: gaussian( 0.1, 50, 6 );
	}

	100%
	{
		blur: gaussian( 0, 0, 0 );
	}
}

