#ModelTest1 
{
	width: 540px;
	height: 400px;
	transform: translateX(128px) translateY(128px) rotateY(45deg);
	border: 2px solid black;
	border-radius: 64px;
	box-shadow: inset #00000099 14px 14px 8px 9px;
	animation-name: ratio-loop; 
	animation-duration: 6.0s;
	animation-timing-function: ease-in-out;
	animation-iteration-count: infinite;
 }
 
@keyframes 'ratio-loop'
{
	0%
	{
		transform: translateX(128px) translateY(128px) rotateY(45deg);
	}


	50%
	{
		transform: translateX(128px) translateY(128px) rotateY(-5deg);
	}

	100%
	{
		transform: translateX(128px) translateY(128px) rotateY(45deg);
		

	}
}

