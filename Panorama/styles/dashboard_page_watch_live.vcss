#WatchLiveGameList
{
	width: 100%; 
	height:100%;

	flow-children: down; 
	overflow: clip scroll;
}

.DataPending #WatchLiveGameList
{
	visibility: collapse;
}

#WaitLiveGameListSpinner
{
	width: 100%;
	height: 100%;

	transition-property: opacity!immediate;
	transition-duration: 0.0s;
	transition-delay: 0s;

	opacity: 0.0;
}

.DataPending #WaitLiveGameListSpinner
{
	transition-property: opacity!immediate;
	transition-duration: 1.0s;
	transition-delay: 2.0s;

	opacity: 1.0;
}

.HasData #WaitLiveGameListSpinner
{
	opacity: 0.0;
}

.WaitPip
{
	width: 16px;
	height: 16px;

	horizontal-align: center;

	animation-name: PipCycle;
	animation-duration: 1.0s;
	animation-timing-function: ease-in-out;
	animation-iteration-count: infinite;
}

@keyframes 'PipCycle'
{
	0%
	{
		box-shadow: baseText -0.4px -0.4px 1px 1px;
	}

	50%
	{
		box-shadow: baseText -1.5px -1.5px 4px 4px;
	}

	100%
	{
		box-shadow: baseText -0.4px -0.4px 1px 1px;
	}
}

#ListWaitPip1
{
	y: 17%;
	animation-delay: 0.0s;
}

#ListWaitPip2
{
	y: 34%;
	animation-delay: 0.1s;
}

#ListWaitPip3
{
	y: 50%;
	animation-delay: 0.2s;
}

#ListWaitPip4
{
	y: 67%;
	animation-delay: 0.3s;
}

#ListWaitPip5
{
	y: 84%;
	animation-delay: 0.4s;
}

