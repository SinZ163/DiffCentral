#DetailsUpvote
{
	flow-children:right;
	margin-right:20px;
	horizontal-align: right;
	vertical-align: top;
	z-index: 500;
}

.LeagueGame #DetailsUpvote,
.NonLeagueGame #DetailsUpvote
{
	visibility: collapse;
}

#DetailsThumbsUp
{
	width:32px;
	height:32px;

	background-image: url("s2r://panorama/images/control_icons/thumbs_up_png.vtex");
	background-size: contain;

	margin-right:5px;
}

#DetailsDownvoteIcon, #DetailsUpvoteIcon {
	width:20px;
	height:20px;

	background-image: url("s2r://panorama/images/control_icons/arrow_vote_psd.vtex");
	background-size: contain;
	wash-color: #708786;

	margin-top: 10px;
	margin-right:5px;
}

#DetailsDownvoteIcon
{
	transform: rotateZ( 180deg );
	margin-right:8px;
}
#DetailsDownvoteIcon:hover , #DetailsUpvoteIcon:hover {
	wash-color: #cccccc;
}

.NonLeagueGame #DetailsThumbsUp
{
	visibility: collapse;
}

#DetailsUpvoteLabel
{
	margin-top:12px;
	vertical-align:middle;
	flow-children: right;
}

.NonLeagueGame #DetailsUpvoteLabel
{
	visibility: collapse;
}

#DetailsWatchLater
{
	flow-children:right;
	margin-right:20px;
	vertical-align: bottom;
	horizontal-align: right;
	margin-bottom:10px;
	z-index: 500;

	visibility: collapse;
}

#DetailsWatchLater:hover {
	color: #cccccc;
	wash-color: #cccccc;
}

.NonLeagueGame #DetailsWatchLater
{
	visibility: collapse;
}

#DetailsPlus
{
	width:20px;
	height:20px;

	background-image: url("s2r://panorama/images/control_icons/plus_png.vtex");
	background-size: contain;
	wash-color: #708786;

	margin-right:5px;
	margin-top: 3px;
}

#DetailsWatchLaterLabel
{
	margin-top:5px;
	vertical-align:middle;
}

#DetailsDotaTV
{
	flow-children:right;
	margin-left:10px;
	margin-right:30px;
}

.NonLeagueGame #DetailsDotaTV
{
	visibility: collapse;
}

#DetailsBracket
{
	width:32px;
	height:32px;

	background-image: url("s2r://panorama/images/control_icons/brackets_psd.vtex");
	background-size: contain;

	margin-right:5px;
}

#DetailsDotaTVLabel
{
	margin-top:5px;
	vertical-align:middle;
}

#DetailsButtons
{
	vertical-align: top;
	horizontal-align: right;

	flow-children: down;

	margin-top: 20px;
	margin-right: 20px;
}

#DetailsMainButton
{
	height: 40px;
	width: 300px;
}

.PreGame #DetailsMainButton
{
	visibility: visible;
}

.NonLeagueGame #DetailsMainButtonLabelPurchase
{
	visibility: visible;
}

.LeagueGame #DetailsMainButtonLabelWatch
{
	visibility: visible;
}

#DetailsSubButton
{
	height: 24px;
	width: 300px;

	wash-color:#bbbbbb;
}

#DetailsSubButton Label
{
	font-size: 14px;
}

.NonLeagueGame #DetailsSubButton
{
	visibility: collapse;
}




#DetailsSpectatingFriends,
#DetailsSpectatingFriends1, 
#DetailsSpectatingFriends2, 
#DetailsSpectatingFriends3, 
#DetailsSpectatingFriends4Plus 
{
	visibility: collapse;
}

.FriendsSpectating1 #DetailsSpectatingFriends1, 
.FriendsSpectating2 #DetailsSpectatingFriends2, 
.FriendsSpectating3 #DetailsSpectatingFriends3, 
.FriendsSpectating4Plus #DetailsSpectatingFriends4Plus
{
	visibility: visible;
}

#DetailsSpectatingFriendsSeparator
{
	visibility: collapse;
}

.FriendsSpectating1 #DetailsSpectatingFriendsSeparator,
.FriendsSpectating2 #DetailsSpectatingFriendsSeparator,
.FriendsSpectating3 #DetailsSpectatingFriendsSeparator,
.FriendsSpectating4Plus #DetailsSpectatingFriendsSeparator
{
	visibility: visible;
}

Label
{
	color: baseText;
	font-size: 20px;
}

#GameModeLabel, .DetailsTitle 
{
	font-size: 14px;
	color: #758d8c;
	text-transform: uppercase;
	letter-spacing: 2px;
}

.DetailsTitle 
{
	vertical-align: middle;
}
.DetailsValue
{
	color: #cccccc;
	font-size: 14px;
	text-transform: uppercase;
	letter-spacing: 2px;
	vertical-align: middle;
}

