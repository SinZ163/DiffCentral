ToggleButton#FavoriteStar
{
    padding: 0px 0px;
    background-image: url("s2r://panorama/images/control_icons/star_favorite_psd.vtex");
    background-repeat: no-repeat;
    background-size: 24px 24px;
	background-position: 93% 50%;
    wash-color: #444444;
	
	background-color: #00000088;
	flow-children: right;
	border-radius: 6px;
	padding: 6px 12px 6px 12px;
	margin-right: 6px;
	margin-bottom: 6px;
	
	transition-property: background-color, wash-color;
	transition-duration: 0.25s;
	transition-timing-function: ease-in-out;
	
	visibility: visible;
}

ToggleButton#FavoriteStar Label
{
    wash-color: white;
}

ToggleButton#FavoriteStar:hover
{
	wash-color: #4582ad;
}

ToggleButton#FavoriteStar:selected
{
	background-image: url("s2r://panorama/images/control_icons/star_favorite_selected_psd.vtex");
	wash-color: #65b9f5;
	background-color: #3b4c5daa;
}

ToggleButton#FavoriteStar:selected:hover
{
	wash-color: #7ee6ff;
}

// hide parts of the toggle button we don't want
ToggleButton#FavoriteStar .TickBox { visibility: collapse; }



