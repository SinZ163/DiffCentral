.VideoPlayArrowCircle
{
	z-index: 1;
	width: 50px;
	height: 50px;
	pre-transform-scale2d: 1;

	horizontal-align: center;
	vertical-align: center;
	border: 3px solid white;
	border-radius: 50%;
	padding: 12px;
	background-size: 50%;
	background-color: #000000aa;
	box-shadow: #000000 -1px -1px 2px 2px;

	transition-property: pre-transform-scale2d, opacity;
	transition-duration: 0.15s;
	transition-timing-function: ease-in-out;

}

.VideoPlayArrowCirclex:hover
{
	pre-transform-scale2d: .95;
}

.VideoPlayArrowx:hover
{
	pre-transform-scale2d: 1;
}

.VideoPlayArrowCirclex:active
{
	pre-transform-scale2d: .35;
	opacity: 0;
}

.VideoPlayArrowCirclex:active
{
	pre-transform-scale2d: 1.15;
}