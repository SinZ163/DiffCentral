Button.CustomButton
{
    width: 320px;
    min-height: 50px;
	max-height: 120px;
    border-radius: 0px;
    horizontal-align: center;
    
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #5Aa15E ), to( #87d69533 ) );  
	background-image: url("s2r://panorama/images/backgrounds/background_play_button_large_2x_png.vtex");
	background-size: 320px 120px;   
    //background-repeat: no-repeat;

	box-shadow: fill #002a6644 -4px -4px 8px 9px;
	
	border-top: 1px solid #ffffff44;
	border-right: 1px solid #00000088;
	border-left: 1px solid #ffffff44;
	border-bottom: 1px solid #00000088;

	transition-property: box-shadow, background-color, opacity, transform;
	transition-delay: 0.0s;
	transition-duration: 0.2s;
	transition-timing-function: ease-in-out;

    visibility: collapse;
}

Button.CustomButton Label
{
	color: white;
	max-width: 318px;

    text-align: center;
    text-transform: uppercase;
    text-overflow: ellipsis;
    margin-top: 2px;

    horizontal-align: center;
    vertical-align: center;
    
    font-size: 28px;
    font-weight: bold;
    
    letter-spacing: 3px;
    //white-space: nowrap;
    
    padding-left: 8px;
    padding-right: 8px;
}

Button.CustomButton:hover
{
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #5Aa15E ), to( #87d695ff ) );  
	box-shadow: fill #414f4daa -4px -4px 8px 8px;
}

Button.CustomButton:hover Label
{
    color: #FFFFFF;
}

Button.CustomButton:active
{
    background-color: white;
}

Button.CustomButton:active Label
{
    color: black;
}

Button.CustomButton:disabled
{
	background-color: #666666;
}

Button.CustomButton:disabled Label
{
    color: #cbcbcb;
}

