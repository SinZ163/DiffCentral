
#TakeoverPageLeft
{
	width: 1920px;
	height: 1080px;
	background-color: black;
	horizontal-align: center;
	vertical-align: top;	
	opacity: 0;
	overflow: clip;
	
	transition-property: width, height, transform, brightness;
	transition-duration: .48s, .48s, .48s, .3s; 
	transition-timing-function: ease-in;
}

#TakeoverPageLeft.ShowDebut
{
	opacity: 1;
}

#FrontpageCellDebutArcanaZeus,
#FrontpageCellDebutArcWarden
{
	width: 336px;
	height: 600px;
	overflow: clip;
}

#FrontpageCellDebutArcWarden #TakeoverPageLeft
{
	width: 736px;
	height: 1000px;
	transform: translateY(-240px)  translateX(180px);
	opacity: 1;
}

#FrontpageCellDebutArcanaZeus #TakeoverPageLeft
{
	width: 368px;
	height: 480px;
	transform: translateX(0px);
	opacity: 1;
}

#FrontpageCellDebutArcanaZeus .DebutCellHeader,
#FrontpageCellDebutArcanaZeus .DebutCellFooter,
#FrontpageCellDebutArcWarden .DebutCellHeader,
#FrontpageCellDebutArcWarden .DebutCellFooter
{
	opacity: 1;
}


#FrontpageCellDebutArcanaZeus #CustomHeader
{
	background-color: black;
	visibility: visible;
	color: #528EA5;	
	font-weight: normal;
	box-shadow: none;
}

#FrontpageCellDebutArcanaZeus #CustomFooterText
{
	color: #cccccc;
	text-shadow: 0px 1px 3px 3.0 #000000;
}

#CustomHeader
{
	box-shadow: fill #69E9E366 -2px -2px 4px 4px;
	background-color: #7AFCFC;
	visibility: visible;
	color: black;
	text-shadow: none;
	font-weight: bold;

	transition-property: color;
	transition-duration: 0.2s;
	transition-timing-function: ease-in-out;
}

#CustomFooterText
{
	visibility: visible;
	color: #7AFCFC;
	transition-property: color;
	transition-duration: 0.2s;
	transition-timing-function: ease-in-out;
	text-shadow: 0px 0px 3px 3.0 #13B37933;
}

#FrontpageCellDebutArcanaZeus:hover #CustomHeader,
#FrontpageCellDebutArcWarden:hover #CustomHeader
{
	color: white;
}

#CustomFooter
{
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #00000000 ), color-stop( 0.8, #0000004f  ), color-stop( 0.85, #0000006f  ), to( #000000ff ) );	
	vertical-align: bottom;
	width: 100%;
	height: 100%;
}

#FrontpageCellDebutArcanaZeus #CustomFooter
{
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #00000000 ), color-stop( 0.8, #0000004f  ), color-stop( 0.85, #0000006f  ), to( #000000ff ) );	
	vertical-align: bottom;
	width: 100%;
	height: 100%;
}

#CustomFooterDetails
{
	visibility: visible;
	vertical-align: bottom;
	margin-bottom: 32px;
	z-index: 1;
	color: #cccccc;
	max-height: 1200px;
}

#FrontpageCellDebutArcanaZeus:hover #CustomFooterDetails,
#FrontpageCellDebutArcWarden:hover #CustomFooterDetails
{
	opacity: 1;
}

#DebutBackgroundImage
{
	width: 100%;
	height: 100%;
	background-image: url("s2r://panorama/images/leaf_pages/debut_arc_warden/background_psd.vtex");
	background-size: cover;
	background-position: 50% 50%;
	background-repeat: no-repeat;	
	
	transition-property: brightness;
	transition-duration: .28s; 
	transition-timing-function: ease-in;
}


#FrontpageCellDebutArcWarden #DebutBackgroundImage
{
	background-size: 120% 53%;
	background-position: 50% 50%;
}


#FrontpageCellDebutArcanaZeus #DebutBackgroundImage
{
	background-image: url("s2r://panorama/images/leaf_pages/debut_arcana_zeus/cell_bg_clouds_psd.vtex");
	
	
}

.DebutArcWarden #DebutInformation
{
	background-color: transparent;
	wash-color: white;
	opacity: 1;
	pre-transform-scale2d: 1;
}

.Initialize.DebutArcWarden #DebutInformation
{
	wash-color: #D681B2;
	opacity: 0;
	pre-transform-scale2d: .92;
}



#DebutInformation
{
	width: 600px;
	height: 800px;
	background-color: #000000cc;
	horizontal-align: center;
	margin-top: 150px;
	margin-left: 790px;

	transition-property: wash-color, opacity, pre-transform-scale2d;
	transition-duration: 2.5s, 2s, 3s; 
	transition-timing-function: ease-in;	
}

#HeroName
{
	font-size: 54px;
	text-transform: uppercase;
	color: white;
	letter-spacing: 3px;
	margin-left: 16px;
	margin-top: -16px;
}

#Model,
#ModelBackground
{
	transform: scaleX(-1);
	width: 100%;
	height: 100%;
	pre-transform-scale2d: 1;
	//background-color: black;
	//blur: gaussian( 1, 1, 1);
	vertical-align: middle;
	horizontal-align: center;
	transition-property: width, height, blur;
	transition-duration: .48s; 
	transition-timing-function: ease-in;	
	background-size: 100% 100%;
	background-position: 50% 50%;
	background-repeat: no-repeat;	
}

#FrontpageCellDebutArcanaZeus #Model,
#FrontpageCellDebutArcanaZeus #ModelBackground,
.ArcanaZeus #Model,
.ArcanaZeus #ModelBackground
{
	transform: scaleX(1);

}

#FrontpageCellDebutArcanaZeus #Model
{
	border-radius: 1px;
}

#FrontpageCellDebutArcanaZeus #ModelBackground,
.DebutArcWarden #ModelBackground,
.ArcanaZeus #ModelBackground
{
	width: 25%;
	height: 25%;
	pre-transform-scale2d: 4;
	blur: gaussian (1,1,1);
}

.ArcanaZeus #ModelBackground
{
	background-image: url("s2r://panorama/images/leaf_pages/debut_arcana_zeus/bg_clouds_psd.vtex");	
}

.DebutArcWarden #ModelBackground
{
	transform: scaleX(1);
	background-image: url("s2r://panorama/images/leaf_pages/debut_arc_warden/background_psd.vtex");
}



#Model1
{
	width: 1920px;
	height: 1080px;
	pre-transform-scale2d: 1;
	horizontal-align: center;
	transform: translateX(-370px) translateY( 0px );
	transition-property: width, height, transform, pre-transform-scale2d;
	transition-duration: .48s; 
	transition-timing-function: ease-in;
	visibility: collapse;
}

.Compact #Model1
{
	width: 1260px;
	height: 640px;
	transform: translateX(-430px) translateY( 20px );
}

#TakeoverPageLeft.Compact
{
	width: 336px;
	height: 480px;

	transform: translateX( -176px) translateY( 152px);
}

.NotOnTodayPage #TakeoverPageLeft
{
	opacity: 1;
}

#FrontpageCellDebutArcanaZeus:hover #DebutBackgroundImage
{
	brightness: 1.8;
}

#FrontpageCellDebutArcWarden:hover #DebutBackgroundImage
{
	brightness: 3;
}


.DebutCellHeader
{
	color: #528EA5;
	background-color: black;
	font-size: 14px;
	margin: 0px;
	padding: 4px 8px 0px 12px;
	text-shadow: 0px 0px 8px 2.0 #000000;
    letter-spacing: 1px;
    text-transform: uppercase;
	font-weight: medium;

	transition-property: color, opacity;
	transition-duration: 0.15s;
	transition-timing-function: ease-in; 	
	opacity: 0;
}

.DebutCellFooter
{
	background-color: #00000026;
	opacity: 1.0;
	width: 100%;
	vertical-align: bottom;
	height: 134px;
	transform: translateY(0px);
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #00000000 ), color-stop( 0.7, #0000003f  ), color-stop( 0.75, #0000003f  ), to( #000000ff ) );
	transition-property: transform,background-color, opacity;
	transition-duration: 0.25s;
	transition-delay: 0.0s;
	transition-timing-function: ease-in-out;
	opacity: 0;
}

.DebutCellTitle
{
	color: #cccccc;
	font-size: 16px;
	margin-left: 12px;
	margin-bottom: 6px;
	vertical-align: bottom;
	text-transform: uppercase;
	letter-spacing: 1px;
	text-shadow: 0px 1px 3px 3.0 #000000;	
	z-index: 1;

}

.Compact .DebutCellFooter, .Compact .DebutCellHeader
{
	opacity: 1;
}

