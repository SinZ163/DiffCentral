/* styles for the partner account balance recharge popup */

PopupConfirmEmoticonUnlock
{
    flow-children: down;
}

#MessageLabel
{
    font-size: 24px;
    color: #FFFFFF;
    horizontal-align: center;
    vertical-align: center;
    max-width: 600px;
    text-align: center;
}

#EmoticonsList
{
    flow-children: right-wrap;
    horizontal-align: center;
    width: 570px;
    margin-top: 8px;
}

.EmoticonContainer
{
    flow-children: right;
}

#EmoticonImage
{
	width: 32px;
	height: 32px;
}

.EmoticonText 
{
    min-width: 100px;
	font-size: 18px;
    vertical-align: center;
    margin-left: 8px;
	color: #aaaaaa;
}

