#PredictionTitle
{
    font-size:30px;
    text-transform:uppercase;
    color:white;
    
}
DOTAPopupPredictionResults
{
    width:800px;
    flow-children:down;
}
#TopHeader
{
    width:100%;
    flow-children:right;
}
#MainSectionContainer
{
    background-color: #333333;
    width:100%;
    flow-children:down;
}
#Header
{
    flow-children:right;
    width:100%;
}
#Rank
{
    width:10%;

}
#Prediction
{
    width: 336px;
}
#Amount,
#Community
{
    width:20%;
}

#Rank, #Prediction, #Amount, #Community {
    vertical-align: middle;
    horizontal-align: right;
    height: 100%;
    text-align: center;
}

#HeroImage
{
    width:80px;
    height:50px;
    horizontal-align: center;
    vertical-align: middle;
}
#HeroContainer
{
    width: 100%;
    horizontal-align: left;
    flow-children: right;
    text-align: center;
}
#HeroNameLabel
{
    horizontal-align: left;
}
.TeamPopup #HeroImage, .PlayerPopup #HeroImage, .TeamPopup #HeroNameLabel, .PlayerPopup #HeroNameLabel
{
    visibility: collapse;
}
.HeroPopup #PlayerTeamImage
{
    visibility: collapse;
}
.HeroPopup .BodyRow
{
    height:50px;
    margin-bottom:5px;
}
#TeamPlayerContainer .AnswerLabel
{
    horizontal-align: left;
}
.HeroPopup .AnswerLabel
{
    visibility:collapse;
}

.TeamPopup .BodyRow,
.PlayerPopup .BodyRow
{
    height:80px;
    margin-bottom:5px;
}
#PlayerTeamImage
{
    width:80px;
    height:80px;
    horizontal-align:center;
}
.ResultText Label
{
    color: #bfd2ee;
    vertical-align: middle;
    horizontal-align: center;
    text-align: center;
    padding-left: 20px;
}
#TeamPlayerContainer
{
    width: 100%;
    horizontal-align: left;
    flow-children: right;
    text-align: center;
}