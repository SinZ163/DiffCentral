@define leftMargin: 118px;

#ManageTeamInfo
{
    width: 740px;
    padding-top: 0px;
    padding-left: 0px;
    padding-right: 0px;
    padding-bottom: 0px;
}

#TeamInfoPanelContainer
{
    background-color: #242424;
    height: fit-children;
    width: 100%;
    flow-children: down;
}

.PageTitle
{
	width: 100%;
    height: 100px;
    background-color: #000000;
    //border-bottom: 1px solid white;
}

#TeamHeaderTeamName
{
    color: white;
	font-size: 20px;
	letter-spacing: 1px;
	text-transform: uppercase;
	margin-bottom: 0px;
    vertical-align: middle;
    horizontal-align: left;
    padding-left: 5px;
    max-width: 325px;
}

#TeamIDInfo
{
    height: fit-children;
    horizontal-align: right;
    padding-right: 100px;
    vertical-align: middle;
}

#TeamHeaderTeamID, #TeamHeaderTeamAdmin
{
    horizontal-align: right;
}

#TeamHeaderTeamIDLabel, #TeamHeaderTeamAdminLabel
{
    horizontal-align: right;
    text-transform: uppercase;
    vertical-align: middle;
    color: #666666;
    padding-right: 5px;
    font-size: 14px;
}

#TeamHeaderTeamIDNumber, #TeamHeaderTeamAdminName
{
    text-transform: uppercase;
    vertical-align: middle;
    color: #FFFFFF;
    font-size: 14px;
}

.TeamHeaderLogo
{
    height: 100px;
    width: 100px;
    margin: 5px;
    border-radius: 7px 7px;
    vertical-align: middle;
}

#TeamSecondaryTabs
{
    width: fill-parent-flow( 1.0 );
    flow-children: right;
    height: 40px;
    margin-left: leftMargin;
    margin-top: 10px;
    margin-bottom: 25px;

    opacity: 1.0;
    transition-property: transform, opacity;
    transition-duration: 0.3s;
    transition-timing-function: ease-in-out;
}

.TeamSecondaryTabButton
{
    margin: 0px;
    vertical-align: middle;
    wash-color: white;

    transition-property: wash-color;
    transition-duration: 0.6s;
    transition-timing-function: ease-in-out;
}

.TeamSecondaryTabButton:active
{
	sound: "ui_rollover_micro";
}

.TeamSecondaryTabSeperator
{
    margin-left: 20px;
    margin-right: 20px;
    vertical-align: middle;
    color: #466480;
}

.TeamSecondaryTabButton Label
{
    vertical-align: middle;
    text-transform: uppercase;
    margin: 0px;
    letter-spacing: 2px;
    color: #466480;
}

.TeamSecondaryTabButton .RadioBox
{
    visibility: collapse;
}

#SlotTypeTabContents
{
    height: 400px;
    width: 100%;
    flow-children: down;
    margin-left: leftMargin;
}

.TabContents
{
    height: fit-children;
    width: 100%;
    flow-children: down;
    visibility: collapse;
    horizontal-align: center;
    opacity: 0.0;
	pre-transform-scale2d: 0.975;
	wash-color: #63b5af;


    transition-property: opacity, pre-transform-scale2d, wash-color;
	transition-duration: 0.32s;
	transition-timing-function: ease-in;
}

.TabContents.TabVisible
{
    visibility: visible;
    opacity: 1.0;
	pre-transform-scale2d: 1.0;
	wash-color: white;
}

.PageSubTitle
{
    horizontal-align: left;
	width: 100%;
	margin-top: 58px;
	margin-bottom: 8px;
}

.PageSubTitle Label
{
    color: #888888;
	font-size: 20px;
}

#TeamLogoContainer
{
    horizontal-align: left;
}

#TeamsContainer
{
    horizontal-align: center;
	width: 100%;
	height: 100%;
	padding-left: leftMargin;
	opacity-mask: url("s2r://panorama/images/masks/challenges_opacity_mask_png.vtex") 1.0;
}

.TeamsContainerRow
{
    horizontal-align: center;
}

.TeamPanelRow
{
    margin-top: 10px;
    margin-bottom: 10px;
    flow-children: right;
    width: 100%;
    height: fit-children;
}

#ImageRow1
{
	margin: 0px;
}

.TeamLogoImage
{
    background-color: #000000;
    border-radius: 5px 5px;
}

#TeamLogoImage0
{
    height: 100px;
    width: 167px;
}

#TeamLogoImage1
{
    height: 100px;
    width: 100px;
}

#TeamLogoImage2
{
    height: 100px;
    width: 100px;
}

#TeamLogoImage3
{
    height: 100px;
    width: 167px;
}

.TeamsInputArea
{
    horizontal-align: left;
    padding-bottom: 10px;
    width: 270px;
    padding-right: 15px;
}

.TeamsInputArea TextEntry
{
    width: 240px;
    height: 32px;
    font-size: 16px;
    background-color: #1d2125;
}

.TeamsInputArea Label
{
    font-size: 14px;
}

.OfficialInfoLabel
{
    text-transform: uppercase;
    letter-spacing: 1px;
}

#TeamNameEntry
{
	vertical-align: top;
}

#TeamTagEntry
{
    vertical-align: top;
}

#TeamCountryEntry
{
    vertical-align: top;
}

#TeamURLEntry
{
    vertical-align: top;
    width: 199px;
}

#TeamURLHTTP
{
    vertical-align: middle;
    color: #FFFFFF;
    font-size: 16px;
}

#NavContainer
{
	horizontal-align: right;
	width: 100%;
	margin-top: 58px;
}

HorizontalScrollBar
{
	opacity: 0;
}


// New from Carollos team info
#ProInfo
{
	width: 100%;

	flow-children: down;

	padding: 0px 20px 0px 20px;
}


#ProInfoPendingPanel Label
{
	horizontal-align: center;
	vertical-align: center;

}

.ProInfoPending #ProInfoPendingPanel
{
	visibility: visible;
}

#ProInfo.TabEnabled
{
	visibility: visible;
}

.ProInfoHeader
{
	padding-top: 20px;
	color: #eeeeee;
}

.NotLocalProfile RadioButton#ProInfoOption,
.NotLocalProfile Label#ProInfoOptionSeparator
{
	visibility: collapse;
}

.ProInfoSub
{
	color: #8e9795;
	font-size: 14px;
	padding-bottom: 4px;
}

#ProInfoResults
{
	width: 100%;
	margin-top: 20px;
	padding: 20px 20px 20px 20px;

	flow-children: down;

	background-color: #000000;
}

#ProInfoHeader
{
	horizontal-align: center;
	text-align: center;
	color: #aaa;
}


#ProInfoPreview
{
	margin-top: 10px;
	margin-bottom: 10px;

	horizontal-align: center;
	text-align: center;
	color: #ffffff;

	font-size: 24px;
	min-height: 20px;
}

#ProInfoButtonsPanel
{
    margin-top: 0px;
}

#ProInfoButtons
{
    horizontal-align: center;
    width: 300px;
    flow-children: right;
}

.ProInfoButtons
{
    width: 100px;
    min-width: 100px;
    margin-left: 10px;
    margin-right: 10px;
}

#ProInfoSubmitPendingLabel,
#ProInfoSubmitSuccessLabel,
#ProInfoSubmitFailureLabel
{
	min-height: 24px;
	height: 24px;

	horizontal-align: center;
	text-align: center;

	visibility: collapse;
}

.ProInfoSubmitPending #ProInfoSubmitPendingLabel,
.ProInfoSubmitSuccess #ProInfoSubmitSuccessLabel,
.ProInfoSubmitFailure #ProInfoSubmitFailureLabel
{
	visibility: visible;
}

.ProInfoSubmitPending #ProInfoSubmit,
.ProInfoSubmitSuccess #ProInfoSubmit,
.ProInfoSubmitFailure #ProInfoSubmit,
.ProInfoSubmitPending #ProInfoCancel,
.ProInfoSubmitSuccess #ProInfoCancel,
.ProInfoSubmitFailure #ProInfoCancel,
.ProInfoSubmitSuccess #ProInfoSubmitPendingLabel,
.ProInfoSubmitFailure #ProInfoSubmitPendingLabel,
.ProInfoSubmitPending #ProInfoSubmitSuccessLabel,
.ProInfoSubmitFailure #ProInfoSubmitSuccessLabel,
.ProInfoSubmitPending #ProInfoSubmitFailureLabel,
.ProInfoSubmitSuccess #ProInfoSubmitFailureLabel
{
	visibility: collapse;
}

#ProInfoResults .ButtonBevel 
{
	min-height: 24px;
	height: 24px;

	horizontal-align: center;
}

#ProInfoResults .ButtonBevel Label
{
	font-size: 14px;
	margin-top: 0px;
}

TextEntry.SmallerEntry
{
	height: 30px;
	width: 320px;

	font-size: 16px;
}

DropDown.SmallerDropdown
{
	height: 30px;
	background-size: 24px 24px;
}

DropDown.SmallerDropdown Label
{
	font-size: 16px;
	margin-top: 0px;
}

#TeamPanelBottom
{
    vertical-align: bottom;
    background-color: #000000;
    height: 100px;
    width: 100%;
    //border-top: 1px solid white;
    padding-right: 25px;
}

.PopupButtonRow
{
    horizontal-align: right;
}

.PopupButtonRow .PopupButton
{
    min-width: 0px;
    width: 120px;
    font-size: 14px;
    visibility: visible;
}

.PopupButton Label
{
    font-size: 18px;
}

#TournamentTeamButton
{
    padding-top: 25px;
}

.TournamentTeamDisclaimer
{
    font-size: 14px;
    color: #666666;
    padding-left: 30px;
    padding-right: 50px;
}

.TeamImageArea
{
    height: 190px;
    width: 300px;
    padding-right: 50px;
}

.TeamImageArea Button
{
    width: 100px;
    min-width: 100px;
    margin-top: 5px;
    height: 30px;
    min-height: 30px;
}

.TeamImageArea Label
{
    font-size: 14px;
    text-transform: uppercase;
}

.TeamImageDescLabel
{
    margin-top: 5px;
}

#RosterHeaderLabel
{
    color: #FFFFFF;
    font-size: 14px;
    text-transform: uppercase;
}

#TeamRosterPanel
{
    background-color: #000000;
    
    height: 275px;
    width: 550px;
    padding-left: 25px;
    padding-right: 25px;
    padding-top: 15px;
    padding-bottom: 10px;
    margin-bottom: 15px;
    flow-children: down;
    overflow: clip scroll;
}

#TeamRosterPanel Label
{
    font-size: 18px;
    vertical-align: middle;
}

.TeamRosterAvatarImage
{
    height: 48px;
    width: 48px;
    padding-right: 5px;
}

.TeamRosterPlayer
{
    width: 100%;
    height: fit-children;
    padding-top: 5px;
    padding-bottom: 5px;
}

#TeamRosterButtons
{
    width: 550px;

}

#TeamInvitePlayerButton
{
    margin-top: 5px;
}

#TeamInvitePlayerButton Label
{
    font-size: 14px;
    text-transform: uppercase;
}

#TeamLeaveTeamButton, #TeamAddSelfButton
{
    horizontal-align: right;
    visibility: collapse;
}

#TeamDisbandTeamButton
{
    horizontal-align: right;
    margin-top: 5px;
}

#TeamAddSelfButton Label
{
    font-size: 14px;
    text-transform: uppercase;
}

#TeamLeaveTeamButton Label, #TeamDisbandTeamButton Label
{
    font-size: 14px;
    text-transform: uppercase;
    color: #ff0000;
}

#TeamSearchInviteTextEntry
{
    width: 320px;
    padding-right: 0px;
}

#TeamInvitePlayerContainer
{
    flow-children: down;
    height: fit-children;
}

#TeamJoinLeaveTeamContainer
{
    flow-children: down;
    height: fit-children;
    horizontal-align: right;
}

.ReduceTeamInfoIcon
{
    height: 14px;
    width: 14px;
    tooltip-position: right;
	tooltip-body-position: 0% 50%;
}

.TeamAdminControls
{
    height: fit-children;
    horizontal-align: right;
    width: 125px;
    flow-children: down;
    vertical-align: middle;
}

#AdminControls Label
{
    horizontal-align: right;
    font-size: 14px;
}

#AdminControls Label:hover
{
    color: #FFFFFF;
}

#TeamInviteAvatarImage
{
    horizontal-align: right;
    vertical-align: middle;
    height: 32px;
    width: 32px;
}

#TeamInviteAutocompleteOption
{
    width: 320px;
    height: 35px;
	flow-children: right;
	background-color: #3d4448;
	color: #e1e1e1;
	font-size: 28px;
	z-index: 0;
	opacity: 1.0;
    
	border-top: 1px solid #00000066;
	border-bottom: 1px solid #00000066;
	
    transition-property: background-color;
	transition-duration: 0.2s;
	transition-timing-function: ease-in-out;
}

#TeamInviteAutocompleteOption:hover, #TeamInviteAutocompleteOption:focus
{
    background-color: #585e62;
}

#TeamInviteAutocompleteOption Label
{
    width: fill-parent-flow(1.0);
	padding-left: 16px;

	color: #ffffff99;
	font-size: 18px;
	font-weight: normal;
    vertical-align: middle;
}

#TeamAutocompleteAvatarImage 
{
    width: 32px;
    height: 32px;
    tooltip-position: left;
    vertical-align: middle;
}