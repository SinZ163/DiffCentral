/* Referenced from code */
@define dragCompleteDuration: 0.2s;
.DragItemsPanel
{
	transition-property: opacity, transform;
	transition-duration: dragCompleteDuration;
	opacity: .95;
}
.DragItemsPanel.DragComplete
{
	opacity: 0.0;
	transform: scale3d( 0.1, 0.1, 0.1 );
}

.ScrollArea
{
	height: 100%;
	width: 32px;
	opacity: 0;
	transition-property: opacity;
	transition-duration: 0.4s;
	vertical-align: center;
	
	background-image: url("s2r://panorama/images/control_icons/arrow_solid_right_png.vtex");
	background-repeat: no-repeat;
	background-position: 50% 50%;
	background-size: 100% 5%;
}

#LeftScrollArea
{
	horizontal-align: left;
	background-color: gradient( linear, 0% 0%, 100% 0%, from( #ffffff88 ), to( #00000000 ) );
	background-color: gradient( radial, 0% 20%, 0% 15%, 90% 50%, from( #47575B99 ), to( #ffffff00 ) );
	background-image: url("s2r://panorama/images/control_icons/arrow_solid_left_png.vtex");
}

#RightScrollArea
{
	horizontal-align: right;
	background-color: gradient( linear, 0% 0%, 100% 0%, from( #00000000 ), to( #ffffff88 ) );
	background-color: gradient( radial, 100% 20%, 0% 15%, 90% 50%, from( #47575B99 ), to( #ffffff00 ) );
}

.DragInProgress.ScratchPadVisible .ScrollArea
{
	opacity: 1;
}

.DragInProgress .ScrollArea
{
	opacity: 0;
}

.CraftingVisible .ScrollArea
{
	height: 60%;
	vertical-align: top;
	background-size: 100% 10%;
}