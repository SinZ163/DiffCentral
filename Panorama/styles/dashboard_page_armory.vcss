{
	height: 770px;
}

.DropWillRemove DOTAEconItem
{
	// Dropping now will remove the item from the Scratchpad.
	border: 2px solid #880000; 
}

DOTAEconItem.EmptySlot
{
	border: 3px solid #9AA6A500;
	box-shadow: none;
	background-color: #00000021;
}

DOTAEconItem.EmptySlot:hover
{
	border: 3px solid #9AA6A500;
	box-shadow: none;
	background-color: #00000041;
	pre-transform-scale2d: 1;
}

DOTAEconItem.New
{
	border: 2px solid #34ff35;
	box-shadow:  #66ff0033 -4px -4px 8px 8px;	 
}

.ScratchPadVisible DOTAEconItem.DragHoverTarget
{
	border: 2px solid #9AA6A5ff;
	background-color: #4A5B6077;
}

.ScratchPadVisible DOTAEconItem.DragHoverTarget.EmptySlot
{
	border: 2px solid #9AA6A5ff;
	background-color: #4A5B6077;
}

.ScratchPadVisible DOTAEconItem.DragHoverTarget.EmptySlot:hover
{
	border: 2px solid #9AA6A5ff;
	background-color: #4A5B6077;
}

.SubCategoryItemContainer DOTAEconItem
{
	transition-property: border, height, width, saturation, brightness, pre-transform-scale2d, wash-color;
	transition-duration: 0.1s, 0.28s;
}

.SubCategoryItemContainer DOTAEconItem.DraggingItem
{
	saturation: 0.0;
}

.SubCategoryItemContainer DOTAEconItem.CraftingItem
{
	saturation: 0.0;
	opacity: .1;
	contrast: 0.75;
}

.SubCategoryItemContainer DOTAEconItem.CraftingItem:hover
{
	saturation: 0.0;
	pre-transform-scale2d: 1;
	contrast: 0.75;
	wash-color: #899aa944;
}

.CraftingVisible .SubCategoryItemContainer
{
	height: 370px;
}

.CraftingVisible.ScratchPadVisible .SubCategoryItemContainer
{
	height: 462px;
}

