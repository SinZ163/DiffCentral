
.EconSetPreview, .PreviewRays
{
	width: 100%;
	height: 100%;
}

.EconSetPreview
{
	opacity: 0.0;

	transition-property: opacity;
	transition-duration: 0.3s;
}
.PreviewVisible1 #EconSetPreview1,
.PreviewVisible2 #EconSetPreview2
{
	opacity: 1.0;
}

.PreviewRays
{
	opacity: 0.0;

	transition-property: opacity;
	transition-duration: 0.3s;
}
.ShowAsPreview .PreviewRays
{
	opacity: 1.0;
}