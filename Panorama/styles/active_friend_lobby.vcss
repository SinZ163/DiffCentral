	
DOTAActiveFriendLobby
{
	flow-children: none;
	width: 100%;
	padding: 4px;
    //margin-bottom: 2px;
    padding-left: 10px;
	
	border-top: 1px solid #000000aa;
	border-bottom: 1px solid #000000aa;
	border-right: 16px solid transparent;
 
	background-color: gradient( linear, 100% 0%, 0% 0%, from( #4b786500 ), color-stop( 0.025, #64c39033 ), color-stop( 0.025, #64c390 ), color-stop( 0.035, #64c390 ), color-stop( 0.35, #64c390 ), color-stop( 0.75, #64c390 ), to( #64c390 ) );
	
	transition-property: background-color, pre-transform-scale2d;
	transition-duration: 0.65s;
	transition-delay: 0.2s;
	transition-timing-function: ease-in-out;
}


.Visible DOTAActiveFriendLobby
{
	background-color: gradient( linear, 100% 0%, 0% 0%, from( #4b786500 ), color-stop( 0.025, #64c39033 ), color-stop( 0.025, #64c390 ), color-stop( 0.035, #64c390 ), color-stop( 0.035, #416f5b ), color-stop( 0.055, #272e31 ), to( #272e31 ) );
	
	transition-delay: 0.5s;
	padding-left: 16px;
}


#FriendOpenLobbiesArea DOTAActiveFriendLobby
{
	background-color: gradient( linear, 100% 0%, 0% 0%, from( #64c390 ), color-stop( 0.01, #64c390 ), color-stop( 0.01, #416f5b ), color-stop( 0.035, #272e31 ), to( #272e31 ) );
	
	transition-delay: 0.5s;
	padding-left: 6px;
	padding-top: 2px;
	padding-bottom: 2px;
	border: 1px solid black;
}

#FriendOpenLobbiesArea DOTAActiveFriendLobby.CustomGameOverview
{
	padding-top: 4px;
	padding-bottom: 1px;
}

DOTATooltipFriendsMenu DOTAActiveFriendLobby
{
	background-color: gradient( linear, 100% 0%, 0% 0%, from( #64c390 ), color-stop( 0.01, #64c390 ), color-stop( 0.01, #416f5b ), color-stop( 0.035, #272e31 ), to( #272e31 ) );
	
	border-right: 0px solid transparent;
	
	height: 52px;
	transition-delay: 0.5s;
	padding-left: 16px;
	padding-top: 2px;
	padding-bottom: 2px;
}

DOTAActiveFriendLobby.FriendMenu
{
	background-color: none;
	border: 1px solid transparent;
	padding: 8px;
	height: 100%;
}

.LobbyDetails
{
	vertical-align: middle;
	flow-children: down;
	width: fill-parent-flow( 1.0 );
}

DOTAActiveFriendLobby.FriendMenu .LobbyDetails
{
	vertical-align: top;
	horizontal-align: right;
	flow-children: none;
	//width: fill-parent-flow( 1.0 );
	//width: 100%;
	height: 100%;
	margin-left: 0px;
}

DOTAActiveFriendLobby .LobbyDetails
{
	vertical-align: top;
	horizontal-align: right;
	flow-children: none;
	width: 100%;
	height: 100%;
}

