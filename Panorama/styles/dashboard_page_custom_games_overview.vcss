.VideoPlayArrow
{
	z-index: 2;
	width: 40px;
	height: 40px;
	horizontal-align: center;
	vertical-align: center;
	background-repeat: no-repeat;
	background-image: url("s2r://panorama/images/control_icons/playbutton_nocircle_psd.vtex");
	background-size: 100% 100%;
	background-position: 0px 0px;
	
		z-index: 2;
	width: 35px;
	height: 35px;
	pre-transform-scale2d: .9;
	
	padding: 12px;
	background-size: 100%;
	background-position: 50% 50%;
	horizontal-align: center;
	vertical-align: center;


	transition-property: pre-transform-scale2d;
	transition-duration: 0.15s;
	transition-timing-function: ease-in-out;
}

.VideoPlayArrow
{
	background-position: 0% 100%;
}

.VideoPlayArrowCircle
{
	z-index: 1;
	width: 70px;
	height: 70px;
	pre-transform-scale2d: 1;

	horizontal-align: center;
	vertical-align: center;
	border: 5px solid white;
	border-radius: 50%;
	padding: 12px;
	background-size: 50%;
	background-color: #000000aa;
	box-shadow: #000000 -1px -1px 2px 2px;

	transition-property: pre-transform-scale2d, opacity;
	transition-duration: 0.15s;
	transition-timing-function: ease-in-out;

}

.PartnerPerfectWorld .VideoPlayArrowCircle
{
	visibility: collapse;
}

.PartnerPerfectWorld .VideoPlayArrow
{
	visibility: collapse;
}

#WorkshopVoteVideo:hover 
{
	wash-color: #00000000;
	saturation: 1;
}

#WorkshopVoteVideo:hover .VideoPlayArrowCircle
{
	pre-transform-scale2d: .95;
}

#WorkshopVoteVideo:hover .VideoPlayArrow
{
	pre-transform-scale2d: 1;
}

#WorkshopVoteVideo:active .VideoPlayArrowCircle
{
	pre-transform-scale2d: .35;
	opacity: 0;
}

#WorkshopVoteVideo:active .VideoPlayArrow
{
	pre-transform-scale2d: 1.15;
}

