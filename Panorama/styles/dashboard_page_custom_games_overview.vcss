.RightArrowButton
{
//	margin-bottom: 8px;
//	margin-right: 8px;

	vertical-align: bottom;
	horizontal-align: right;
	
	transition-property: wash-color;
	transition-delay: 0.0s;
	transition-duration: 0.25s;
	transition-timing-function: ease-in-out;
}

.SuggestedGamesBox .RightArrowButton
{
	margin-bottom: 12px;
	margin-right: 12px;
}

.RightArrowButtonIcon
{
	width: 16px;
	height: 16px;
	vertical-align: middle;
	horizontal-align: right;
	background-image: url("s2r://panorama/images/control_icons/double_arrow_right_png.vtex");
    background-position: 0% 50%;
	background-size: contain;
	background-repeat: no-repeat;
	wash-color: #91aba6;
	
	transform: translateX( -12px );
	
	transition-property: transform, wash-color;
	transition-delay: 0.0s;
	transition-duration: 0.25s;
	transition-timing-function: ease-in-out;
}

.RightArrowButton:hover
{
}

.RightArrowButton:hover .RightArrowButtonIcon
{
	wash-color: white;
	transform: translateX( -6px );
}

.RightArrowButton Label
{
	font-size: 20px;
	letter-spacing: 2px;
	font-weight: thin;
	color: #999999;
	text-transform: uppercase;
	margin-top: 4px;
	margin-right: 32px;

	
	transition-property: color;
	transition-delay: 0.0s;
	transition-duration: 0.25s;
	transition-timing-function: ease-in-out;
}


.RightArrowButton:hover Label
{
    color: white;
}

