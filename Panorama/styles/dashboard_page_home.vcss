}


// Footer
//---------------

.DOTATodayCellFooter
{
	background-color: #00000026;
	opacity: 1.0;
	width: 100%;
	vertical-align: bottom;
	height: 134px;
	transform: translateY(0px);
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #00000000 ), color-stop( 0.7, #0000003f  ), color-stop( 0.75, #0000003f  ), to( #000000ff ) );
	transition-property: transform,background-color;
	transition-duration: 0.25s;
	transition-delay: 0.0s;
	transition-timing-function: ease-in-out;
}

#FrontpageSingleitemTreasure .DOTATodayCellFooter,
#FrontpageSetTreasure .DOTATodayCellFooter
{
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #00000000 ), color-stop( 0.7, #00000000  ), color-stop( 0.75, #00000000  ), to( #00000055 ) );
}


#FrontpageSingleitemTreasure:hover .DOTATodayCellFooter,
#FrontpageSetTreasure:hover .DOTATodayCellFooter
{
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #00000000 ), color-stop( 0.7, #000000df  ), color-stop( 0.75, #000000df  ), to( #000000ff ) );	
}

.DOTATodayCell:hover .DOTATodayCellFooter
{
	transform: translateY(0px);
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #00000000 ), color-stop( 0.7, #000000df  ), color-stop( 0.75, #000000df  ), to( #000000ff ) );	
}

.DOTATodayCell.NoDetails:hover .DOTATodayCellFooter
{
	transform: translateY(0px);
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #00000000 ), color-stop( 0.3, #0000003f  ) , color-stop( 0.75, #000000df  ), to( #000000ff ) );	
}

