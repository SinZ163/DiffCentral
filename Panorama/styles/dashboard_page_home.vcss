#TodayPages
{
	width: fit-children;
	margin-top: 66px;
	flow-children: right;
	horizontal-align: center;
	opacity-mask: url("s2r://panorama/images/masks/init_mask_01_psd.vtex") 0;
	opacity: 1;
	pre-transform-scale2d: 1;
	wash-color: white;

	// push our width up so we center in the grid
	margin-left: -14px;

	transition-property: opacity-mask, opacity, pre-transform-scale2d, wash-color;
    transition-duration: 1.2s;
    transition-timing-function: ease-in-out;
}


DOTADashboard.InitialDisplay #TodayPages
{
	opacity-mask: url("s2r://panorama/images/masks/init_mask_01_psd.vtex") 1.0;
