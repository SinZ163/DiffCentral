// Footer Details
//---------------

.CellFooterDetails
{
	width: 100%;
	background-color: gradient( linear, 0% 0%, 0% 100%, from( #00000000 ), color-stop( 0.3, #00000099  ), color-stop( 0.75, #00000099  ), to( #00000000 ) );
	color: #808f8d;
	font-size: 16px;
	font-weight: thin;
	padding-left: 12px;
	margin-bottom: 32px;
//	wash-color: black;
	opacity: 0.0;
	vertical-align: bottom;
	text-shadow: 0px 0px 4px 5.0 #000000;
	max-height: 60px;

	transition-property: wash-color, opacity;
	transition-duration: 0.25s;
	transition-delay: 0.0s;
	transition-timing-function: linear;
}

.DOTATodayCellLarge .CellFooterDetails
{
	margin-top: 52px;
	margin-left: 13px;
}

