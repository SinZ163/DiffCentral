#AHCHeroGrid DOTAHeroImage
{
	width: 34px;
	height: 34px;
	border: 1px solid black;
	margin-right: 5px;

	saturation: 0.8;
	brightness: 1.7;
}

#AHCHeroGrid DOTAHeroImage.Completed
{
	saturation: 0.0;
	brightness: 0.2;
}

#AHCHeroGrid DOTAHeroImage.Active
{
	animation-name: ActiveHeroIconPulse;
	animation-duration: 1.0s;
	animation-timing-function: linear;
	animation-iteration-count: infinite;
}

@keyframes 'ActiveHeroIconPulse'
{
	0%
	{
		box-shadow: 0px 0px 10px #33FF88AA;
		saturation: 0.7;
		brightness: 3.5;
		wash-color: #33FFAACC;
	}
	
	50%
	{
		box-shadow: 0px 0px 17px #33FF88CC;
		saturation: 0.4;
		brightness: 3.8;
		wash-color: #33FFAACC;
	}
	
	100%
	{
		box-shadow: 0px 0px 10px #33FF88AA;
		saturation: 0.7;
		brightness: 3.5;
		wash-color: #33FFAACC;
	}
}

