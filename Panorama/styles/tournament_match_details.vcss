#MatchDetailsGeneralInfo
{
	flow-children:down;
	vertical-align:bottom;
	padding: 10px;
	border-top:1px solid #454d4f;
	width:100%;
	background-color: #212627;

	visibility: collapse;
}

#MatchDetailsGeneralInfo Label
{
    text-align:center;
    width:100%;
	color: #657979;
	font-size: 16px;
}

.MatchDetailsWinner
{
    horizontal-align:center;
}
.TournamentMatchDetails
{
    width: 380px;
    max-height:600px;
    flow-children: down;
    overflow: squish scroll;
}
#MatchDetails
{
    flow-children:down;
    
    width:350px;
    max-height:400px;
    background-color:#2b3335;
    border-top:1px solid #454d4f;
    border-bottom:1px solid #454d4f;
}
#MatchDetailsVersus
{
    flow-children:right;
    width:100%;
    padding: 10px 5px 5px 5px;
}
#MatchDetailsTeam1, #MatchDetailsTeam2
{
    width: fill-parent-flow( 0.3 );
	height: 64px;
}

#MatchDetailsTeam1Logo, #MatchDetailsTeam2Logo
{
    horizontal-align:center;

    width:64px;
    height:64px;
    margin-bottom: 5px;

	visibility: collapse;
}
.HasTeam1 #MatchDetailsTeam1Logo, .HasTeam2 #MatchDetailsTeam2Logo
{
	visibility: visible;
}

.UnknownTeamLabel
{
	text-align: center;
	horizontal-align: center;
	font-size: 28px;
	color: #768e8d;
	font-weight: bold;
	vertical-align: middle;

	visibility: visible;
}
.HasTeam1 #MatchDetailsTeam1 .UnknownTeamLabel, .HasTeam2 #MatchDetailsTeam2 .UnknownTeamLabel
{
	visibility: collapse;
}

#MatchDetailsVsLabel
{
    horizontal-align:center;
    width: fill-parent-flow( 0.2 );
    vertical-align:center;
    text-align:center;
    font-size: 20px;
}

#MatchDetailsGames
{
    width:100%;
    flow-children:down;

    visibility: collapse;
}
.HasTournamentGames #MatchDetailsGames
{
    visibility: visible;
}

.TournamentMatchDetailsGame
{
    width:100%;
    border-top: 1px solid #454d4f;
}

#UpcomingMatchWinnerChampion,
#UpcomingMatchWinnerGoes,
#UpcomingMatchLoserGoes,
#UpcomingMatchLoserEliminated,
#LiveMatchWatch,
#InProgress
{
	visibility: collapse;
}

.WinnerAdvances #UpcomingMatchWinnerGoes,
.WinnerChampion #UpcomingMatchWinnerChampion,
.LoserContinues #UpcomingMatchLoserGoes,
.LoserEliminated #UpcomingMatchLoserEliminated
{
	visibility: visible;
}

.WinnerAdvances #MatchDetailsGeneralInfo,
.WinnerChampion #MatchDetailsGeneralInfo,
.LoserContinues #MatchDetailsGeneralInfo,
.LoserEliminated #MatchDetailsGeneralInfo
{
    visibility: visible;
}

#StageName
{
	text-transform: uppercase;
	font-size: 18px;
	color: #ccc;
	letter-spacing: 2px;
	horizontal-align: center;
}

#BestOf
{
	margin-bottom: 6px;
	horizontal-align: center;
	text-transform: uppercase;
	letter-spacing: 1px;
	font-size: 14px;
	color: #999;

	visibility: collapse;
}

.BestOfSeries #BestOf
{
	visibility: visible;
}