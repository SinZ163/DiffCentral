

DOTACompShanghai2016TeamsPage .CompendiumBody
{
	width: 1392px;
	height: 700px;

	margin-top: 215px;
	border-top: 0;
	border-bottom: 0;
	background-color: transparent;

	horizontal-align: center;
}

.DataPending #TeamSelector,
.DataPending #TeamContainer
{
	visibility: collapse;
}

#TeamContainer,
#TeamSelector
{
	opacity: 0.0;
}

.DisplayTeam #TeamContainer,
.DisplayTeamSelector #TeamSelector
{
	opacity: 1;
}

#TeamSelector
{
	flow-children: down;

	transition-property: opacity;
	transition-duration: 0.2s;
	transition-timing-function: ease-in-out;
}

.DisplayTeamSelector #TeamSelector
{
	opacity: 1;
}

#TeamSelector #TeamLogo
{
	width: 128px;
	height: 128px;

	margin-left: 15px;
	margin-right: 15px;
	margin-bottom: 5px;
}

.LogoMoving #TeamSelector #TeamLogo
{
	visibility: collapse;
}

#TeamSelector .TeamLogoBG
{
	width: 100%;
	height:100%;

	border-bottom: 1px solid #497ca8;
}

#TeamSelector .Team:hover .TeamLogoBG
{
	background-color: #00000000;
}


#TeamSelector .Team
{
	flow-children: down;
	horizontal-align: center;

	margin-left: 40px;
	margin-right: 40px;
}

#TeamSelector Label
{
	width: 100%;
	height: 45px;
	text-align: center;

	margin-top: 3px;

	text-transform: uppercase;
	font-size: 15px;
	letter-spacing: 1.5px;

	transition-property: color;
	transition-duration: 0.1s;
	transition-timing-function: ease-in;

	color: #497ca8;
}

#TeamSelector .Team:hover Label
{
	color: white;
}

#TeamContainer
{
	width: 100%;
	overflow: clip scroll;

	opacity: 0;
	transition-property: opacity;
	transition-duration: 0.2s;
	transition-timing-function: ease-in-out;
}


#TeamContainer #Header
{
	width: 100%;

	margin-top: 20px;
	margin-left: 120px;
	margin-right: 120px;

	background-color: #082736;
}

#TeamContainer #Header #NameAndStatusBlock
{
	margin-top: 15px;
	margin-left: 20px;
	width: 600px;
}

